<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <record id="licenses_menu" model="ir.ui.menu">
        <field name="name">Licenses</field>
        <field name="web_icon">fa fa-code,#0079BF,#FFFFFF</field>
    </record>
    <record id="licenses_by_software_menu" model="ir.ui.menu">
        <field name="name">Licenses</field>
        <field name="sequence">11</field>
        <field name="parent_id" ref="licenses_menu" />
    </record>
    <record id="all_licenses_menu" model="ir.ui.menu">
        <field name="name">All Licenses</field>
        <field name="sequence">12</field>
        <field name="parent_id" ref="licenses_by_software_menu" />
        <field name="action" ref="all_licenses_act_window" />
    </record>
    <record id="software_menu" model="ir.ui.menu">
        <field name="name">Software</field>
        <field name="sequence">1</field>
        <field name="parent_id" ref="licenses_menu" />
        <field name="action" ref="by_software_act_window" />
    </record>
    <record id="licenses_to_review_menu" model="ir.ui.menu">
        <field name="action" ref="to_review_act_window"/>
        <field name="name">To Review</field>
        <field name="parent_id" ref="licenses_by_software_menu"/>
        <field name="sequence">2</field>
    </record>
</odoo>
