<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
  <record id="calendar_event_2" model="calendar.event" context="{'no_mail_to_attendees': True}">
    <field name="name">Olivia Ramirez - Therapy Session Booking</field>
    <field name="location">Mental Therapy, 1160, Belgium</field>
    <field name="appointment_type_id" ref="wellness_practitioner.appointment_type_1"/>
    <field name="duration">1.0</field>
    <field name="start" model="appointment.type" eval="pytz.timezone(obj().env.ref('wellness_practitioner.appointment_type_1').appointment_tz).localize(
            DateTime.today().date() + relativedelta(days=1, hours=10)
        ).astimezone(pytz.UTC).replace(tzinfo=None)"/>
    <field name="stop" model="appointment.type" eval="pytz.timezone(obj().env.ref('wellness_practitioner.appointment_type_1').appointment_tz).localize(
            DateTime.today().date() + relativedelta(days=1, hours=11)
        ).astimezone(pytz.UTC).replace(tzinfo=None)"/>
    <field name="start_date" eval="DateTime.today().date()"/>
    <field name="partner_ids" eval="[(6, 0, [ref('base.partner_admin'), ref('res_partner_2')])]"/>
    <field name="appointment_status">booked</field>
    <field name="appointment_booker_id" ref="res_partner_2"/>
    <field name="alarm_ids" eval="[(6, 0, [ref('calendar.alarm_notif_1'), ref('calendar.alarm_mail_1'), ref('appointment_sms.calendar_alarm_data_1h_sms'), ref('wellness_practitioner.alarm_mail_1')])]"/>
    <field name="appointment_invite_id" ref="wellness_practitioner.appointment_invite_wellness_session"/>
    <field name="opportunity_id" ref="crm_lead_13"/>
  </record>
  <record id="calendar_event_3" model="calendar.event" context="{'no_mail_to_attendees': True}">
    <field name="partner_ids" eval="[(6, 0, [ref('wellness_practitioner.res_partner_15'), ref('base.partner_admin')])]"/>
    <field name="name">Therapy Session</field>
    <field name="duration">1.0</field>
    <field name="start" model="appointment.type" eval="pytz.timezone(obj().env.ref('wellness_practitioner.appointment_type_1').appointment_tz).localize(
            DateTime.today().date() + relativedelta(days=2, hours=15)
        ).astimezone(pytz.UTC).replace(tzinfo=None)"/>
    <field name="stop" model="appointment.type" eval="pytz.timezone(obj().env.ref('wellness_practitioner.appointment_type_1').appointment_tz).localize(
            DateTime.today().date() + relativedelta(days=2, hours=16)
        ).astimezone(pytz.UTC).replace(tzinfo=None)"/>
    <field name="recurrency" eval="True"/>
    <field name="follow_recurrence" eval="True"/>
    <field name="rrule_type">weekly</field>
    <field name="end_type">count</field>
    <field name="count">10</field>
    <field name="thu" eval="True"/>
  </record>
</odoo>
