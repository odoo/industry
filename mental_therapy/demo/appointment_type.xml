<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
  <record id="wellness_practitioner.appointment_type_1" model="appointment.type" forcecreate="1">
    <field name="name">Therapy Session</field>
    <field name="location_id" ref="base.main_partner"/>
    <field name="event_videocall_source" eval="False"/>
    <field name="category">recurring</field>
    <field name="min_cancellation_hours">48.0</field>
    <field name="min_schedule_hours">12.0</field>
    <field name="reminder_ids" eval="[(6, 0, [ref('calendar.alarm_notif_1'), ref('calendar.alarm_mail_1'), ref('appointment_sms.calendar_alarm_data_1h_sms'), ref('wellness_practitioner.alarm_mail_1')])]"/>
    <field name="appointment_invite_ids" eval="[(6, 0, [ref('wellness_practitioner.appointment_invite_wellness_session')])]"/>
    <field name="lead_create" eval="True"/>
    <field name="has_payment_step" eval="False"/>
    <field name="appointment_tz" model="res.users" eval="obj().env.ref('base.user_admin').tz or 'Europe/Brussels'"/>
  </record>
</odoo>
