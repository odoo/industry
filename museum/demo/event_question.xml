<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <function model="event.question" name="write">
        <value model="event.question" eval="obj().search([('title', '=', 'Name'), ('question_type', '=', 'name'), ('event_id', '=', ref('event_event_4'))]).ids"/>
        <value eval="{'is_mandatory_answer': False, 'once_per_order': True}"/>
    </function>
    <function model="event.question" name="write">
        <value model="event.question" eval="obj().search([('title', '=', 'Email'), ('question_type', '=', 'email'), ('event_id', '=', ref('event_event_4'))]).ids"/>
        <value eval="{'once_per_order': True}"/>
    </function>
    <function model="event.question" name="write">
        <value model="event.question" eval="obj().search([('title', '=', 'Phone'), ('question_type', '=', 'phone'), ('event_id', '=', ref('event_event_4'))]).ids"/>
        <value eval="{'once_per_order': True}"/>
    </function>
</odoo>
