<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
  <function model="pos.config" name="load_onboarding_retail_scenario" />
  <record id="point_of_sale.pos_config_retail" model="pos.config" forcecreate="1">
    <field name="name">Museum Entrance</field>
    <field name="iface_available_categ_ids" eval="[(6, 0, [ref('pos_category_6'), ref('pos_category_1')])]"/>
    <field name="payment_method_ids" eval="[(6, 0, [ref('pos_payment_method_3')])]"/>
    <field name="limit_categories" eval="True"/>
  </record>
  <record id="pos_config_2" model="pos.config">
    <field name="name">Museum Shop</field>
    <field name="iface_available_categ_ids" eval="[(6, 0, [ref('pos_category_4')])]"/>
    <field name="limit_categories" eval="True"/>
    <field name="payment_method_ids" eval="[(6, 0, [ref('pos_payment_method_2'), ref('pos_payment_method_1')])]"/>
  </record>
</odoo>
