<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="res_config_account_setting_museum" model="res.config.settings">
        <field name="group_product_pricelist" eval="1"/>
        <field name="use_event_barcode" eval="1"/>
        <field name="group_stock_multi_locations" eval="1"/>
        <field name="group_stock_adv_location" eval="1"/>
    </record>
    <function name="write" model="res.config.settings">
        <value eval="[ref('res_config_account_setting_museum')]"/>
        <value model="res.company" eval="{'account_price_include': 'tax_included'} if not obj().env.company._existing_accounting() else {}"/>
    </function>
    <function name="execute" model="res.config.settings" eval="[ref('res_config_account_setting_museum')]"/>
</odoo>
