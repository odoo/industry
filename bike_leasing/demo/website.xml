<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="website.default_website" model="website">
        <field name="name">My Website</field>
        <field name="salesteam_id" ref="sales_team.salesteam_website_sales"/>
        <field name="configurator_done" eval="True"/>
        <field name="cart_recovery_mail_template_id" eval="False"/>
        <field name="logo" type="base64" file="bike_leasing/static/src/binary/website/1-logo"/>
        <field name="theme_id" search="[('id', '=', ref('base.module_theme_vehicle', raise_if_not_found=False))]"/>
        <field name="language_ids" eval="[(6, 0, [ref('base.lang_en')])]"/>
        <field name="add_to_cart_action">go_to_cart</field>
        <field name="default_lang_id" ref="base.lang_en"/>
    </record>
</odoo>
