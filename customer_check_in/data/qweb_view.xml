<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template inherit_id="event.event_report_template_full_page_ticket" id="check_in_event_report_template_full_page_ticket">
        <xpath expr="//span[@t-field='attendee.event_begin_date']" position="attributes">
            <attribute name="t-field">attendee.x_displayed_start_date</attribute>
        </xpath>
        <xpath expr="//span[@t-field='attendee.event_end_date']" position="attributes">
            <attribute name="t-field">attendee.x_displayed_end_date</attribute>
        </xpath>
    </template>
</odoo>
