<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="calendar_event_1" model="calendar.event" context="{'no_mail_to_attendees': True}">
        <field name="name">John Smith - 60-min Personal Training Session Booking</field>
        <field name="start_date" eval="DateTime.today().date()"/>
        <field name="appointment_type_id" ref="appointment_type_1"/>
        <field name="appointment_booker_id" ref="base.partner_admin"/>
        <field name="partner_ids" eval="[(6, 0, [ref('base.partner_admin')])]"/>
        <field name="start" model="appointment.type" eval="(DateTime.today() + relativedelta(days=1, hours=9)).replace(tzinfo=pytz.timezone(obj().env.ref('personal_trainer.appointment_type_1').appointment_tz)).astimezone(pytz.UTC).replace(tzinfo=None)"/>
        <field name="stop" model="appointment.type" eval="(DateTime.today() + relativedelta(days=1, hours=10)).replace(tzinfo=pytz.timezone(obj().env.ref('personal_trainer.appointment_type_1').appointment_tz)).astimezone(pytz.UTC).replace(tzinfo=None)"/>
        <field name="description"><![CDATA[
            <ul>
                <li>Phone: +000</li>
                <li>Email: John.smith@email.com</li>
            </ul>
        ]]></field>
    </record>
