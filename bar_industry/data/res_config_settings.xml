<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="res_config_settings_enable" model="res.config.settings">
        <field name="group_uom" eval="1"/>
        <field name="group_product_pricelist" eval="True"/>
        <field name="pos_use_pricelist" eval="1"/>
        <field name="pos_self_ordering_mode" eval="'mobile'"/>
        <field name="pos_iface_tipproduct" eval="True"/>
        <field name="pos_show_product_images" eval="True"/>
        <field name="attendance_kiosk_use_pin" eval="True"/>
        <field name="attendance_kiosk_mode">manual</field>
    </record>
    <function name="write" model="res.config.settings">
        <value eval="[ref('res_config_settings_enable')]"/>
        <value model="res.company" eval="{'account_price_include': 'tax_included'} if not obj().env.company._existing_accounting() else {}"/>
    </function>
    <function model="res.config.settings" name="execute" eval="[ref('res_config_settings_enable')]"/>
</odoo>
