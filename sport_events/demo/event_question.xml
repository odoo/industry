<?xml version='1.0' encoding='UTF-8'?>
<odoo auto_sequence="1" noupdate="1">
  <function model="event.question" name="write">
    <value model="event.question" eval="obj().search([
      ('event_ids', 'in', [ref('event_event_1'), ref('event_event_3')]),
      ('question_type', '=', 'phone')
    ]).ids"/>
    <value eval="{'title': 'Emergency Contact', 'is_mandatory_answer': True, 'event_ids': [Command.clear(), Command.link(ref('event_event_1')), Command.link(ref('event_event_3'))]}"/>
  </function>
  <record id="event_question_5" model="event.question">
    <field name="title">T-Shirt Size</field>
    <field name="event_ids" eval="[Command.clear(), Command.link(ref('event_event_1'))]"/>
    <field name="is_mandatory_answer" eval="True"/>
  </record>
</odoo>
