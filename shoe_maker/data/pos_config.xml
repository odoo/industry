<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <function model="pos.config" name="load_onboarding_retail_scenario">
        <value name="with_demo_data" model="ir.module.module" eval="obj().search([('name', '=', 'library')]).demo"/>
    </function>
    <record id="point_of_sale.pos_config_retail" model="pos.config" forcecreate="1">
        <field name="name">Front desk</field>
        <field name="iface_available_categ_ids" eval="[(6, 0, [ref('pos_category_2'), ref('pos_category_1'), ref('pos_category_4'), ref('pos_category_3')])]"/>
        <field name="limit_categories" eval="True"/>
    </record>
</odoo>
