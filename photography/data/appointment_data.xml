<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="reminder_mail_1d" model="calendar.alarm">
        <field name="name">1 day before</field>
        <field name="duration">1</field>
        <field name="interval">days</field>
        <field name="alarm_type">email</field>
        <field name="mail_template_id" ref="email_calendar_reminder" />
    </record>

    <record id="appointment_studio" model="appointment.type">
        <field name="name">Studio Shoot</field>
        <field name="appointment_duration">1.5</field>
        <field name="image_1920" type="base64" file="photography/static/src/binary/appointment_type/1-image_1920"/>
        <field name="appointment_tz" model="res.users" eval="obj().env.ref('base.user_admin').tz or 'Europe/Brussels'"/>
        <field name="min_schedule_hours">72</field>
        <field name="min_cancellation_hours">72</field>
        <field name="max_schedule_days">90</field>
        <field name="location_id" ref="base.main_partner"/>
        <field name="staff_user_ids" eval="[(4, ref('base.user_admin'))]" />
        <field name="reminder_ids" eval="[(4, ref('reminder_mail_1d'))]" />
        <field name="assign_method">time_auto_assign</field>
        <field name="slot_ids" eval="[
        (0, 0, {'weekday': '1', 'start_hour': 9.0, 'end_hour': 12.0}),
        (0, 0, {'weekday': '1', 'start_hour': 14.0, 'end_hour': 17.0}),
        (0, 0, {'weekday': '2', 'start_hour': 9.0, 'end_hour': 12.0}),
        (0, 0, {'weekday': '2', 'start_hour': 14.0, 'end_hour': 17.0}),
        (0, 0, {'weekday': '3', 'start_hour': 9.0, 'end_hour': 12.0}),
        (0, 0, {'weekday': '3', 'start_hour': 14.0, 'end_hour': 17.0}),
        (0, 0, {'weekday': '4', 'start_hour': 9.0, 'end_hour': 12.0}),
        (0, 0, {'weekday': '4', 'start_hour': 14.0, 'end_hour': 17.0}),
        (0, 0, {'weekday': '5', 'start_hour': 9.0, 'end_hour': 12.0}),
        (0, 0, {'weekday': '5', 'start_hour': 14.0, 'end_hour': 17.0}),
        ]"/>
    </record>

    <record id="appointment_type_2" model="appointment.type">
        <field name="name">Portrait Session</field>
        <field name="image_1920" type="base64" file="photography/static/src/binary/appointment_type/2-image_1920"/>
