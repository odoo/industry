<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="project_remark_type_1" model="project.task.type">
        <field name="name">New</field>
        <field name="x_is_remark_stage" eval="True"/>
        <field name="active" eval="True"/>
        <field name="user_id" eval="False"/>
    </record>
    <record id="project_remark_type_2" model="project.task.type">
        <field name="name">In progress</field>
        <field name="x_is_remark_stage" eval="True"/>
        <field name="active" eval="True"/>
        <field name="user_id" eval="False"/>
    </record>
    <record id="project_remark_type_3" model="project.task.type">
        <field name="name">Done</field>
        <field name="sequence">2</field>
        <field name="fold" eval="True"/>
        <field name="x_is_remark_stage" eval="True"/>
        <field name="active" eval="True"/>
        <field name="user_id" eval="False"/>
  </record>
</odoo>
