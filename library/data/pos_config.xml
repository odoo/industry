<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <function model="pos.config" name="load_onboarding_retail_scenario">
        <value name="with_demo_data" model="ir.module.module" eval="obj().search([('name', '=', 'library')]).demo"/>
    </function>
    <record id="point_of_sale.pos_config_retail" model="pos.config" forcecreate="1">
        <field name="name">Library</field>
        <field name="available_pricelist_ids" eval="[(6, 0, [ref('product_pricelist_2')])]"/>
        <field name="tip_product_id" eval="False"/>
        <field name="advanced_employee_ids" eval="[(6, 0, [ref('hr.employee_admin')])]"/>
        <field name="use_pricelist" eval="True"/>
    </record>
</odoo>
