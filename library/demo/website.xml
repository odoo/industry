<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
  <record id="website.default_website" model="website" forcecreate="1">
    <field name="name">Library</field>
    <field name="configurator_done" eval="True"/>
    <field name="logo" type="base64" file="library/static/description/icon.png"/>
    <field name="user_id" ref="base.public_user"/>
    <field name="favicon" type="base64" file="library/static/description/icon.png"/>
    <field name="theme_id" search="[('id', '=', ref('base.module_theme_bewise', raise_if_not_found=False))]"/>
    <field name="salesperson_id" ref="base.user_admin"/>
    <field name="show_line_subtotals_tax_selection">tax_excluded</field>
    <field name="account_on_checkout">mandatory</field>
    <field name="cart_recovery_mail_template_id" eval="False"/>
    <field name="shop_ppr">5</field>
    <field name="shop_page_container">fluid</field>
    <field name="shop_opt_products_design_classes">o_wsale_products_opt_thumb_2_3</field>
    <field name="product_page_image_width">66_pc</field>
  </record>
</odoo>
