<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <record id="ir_cron_load_ademe_data" model="ir.cron">
        <field name="name">Load ADEME Data</field>
        <field name="model_id" ref="esg.model_esg_database"/>
        <field name="state">code</field>
        <field name="code"><![CDATA[
model.env.ref('esg.esg_database_ademe').action_load_data()
model.env['ir.cron']._commit_progress(processed=1, remaining=0, deactivate=True)
        ]]></field>
        <field name="interval_number">1</field>
        <field name="interval_type">minutes</field>
        <field name="nextcall" eval="datetime.today().date()+relativedelta(minutes=1)"/>
    </record>
</odoo>
