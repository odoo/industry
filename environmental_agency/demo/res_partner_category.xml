<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="base_industry_data.res_partner_category_6" model="res.partner.category" forcecreate="0">
        <field name="partner_ids" model="hr.employee" eval="[(6, 0, [obj().env.ref('environmental_agency.hr_employee_3').work_contact_id.id])]"/>
    </record>
    <record id="base_industry_data.res_partner_category_11" model="res.partner.category" forcecreate="0">
        <field name="partner_ids" model="hr.employee" eval="[(6, 0, obj().search([('work_contact_id', '!=', False)]).mapped('work_contact_id.id'))]"/>
    </record>
    <function model="res.partner" name="write">
        <value model="res.partner" eval="obj().search([('category_id', 'in', ref('base_industry_data.res_partner_category_11'))]).ids"/>
        <value eval="{'parent_id': ref('base.main_partner')}"/>
    </function>
</odoo>
