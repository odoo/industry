<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="calendar_event_1" model="calendar.event" context="{'no_mail_to_attendees': True}">
        <field name="name">Discovery Call Booking</field>
        <field name="start" model="appointment.type" eval="
            (DateTime.today().date() + relativedelta(weekday=0, hour=14)).replace(tzinfo=pytz.timezone(obj().env.ref('environmental_agency.appointment_type_2').appointment_tz)).astimezone(pytz.UTC).replace(tzinfo=None)
        "/>
        <field name="stop" model="appointment.type" eval="
            (DateTime.today().date() + relativedelta(weekday=0, hour=14, minute=30)).replace(tzinfo=pytz.timezone(obj().env.ref('environmental_agency.appointment_type_2').appointment_tz)).astimezone(pytz.UTC).replace(tzinfo=None)
        "/>
        <field name="appointment_type_id" ref="appointment_type_2"/>
    </record>
</odoo>
