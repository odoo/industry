<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="website.default_website" model="website">
        <field name="name">Architect</field>
        <field name="theme_id" search="[('id', '=', ref('base.module_theme_nano', raise_if_not_found=False))]"/>
        <field name="configurator_done" eval="True"/>
        <field name="logo" type="base64" file="architects/static/description/icon.png"/>
        <field name="favicon" type="base64" file="architects/static/description/icon.png"/>
    </record>
</odoo>
