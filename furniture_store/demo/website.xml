<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="website.default_website" model="website">
        <field name="name">Furniture Store</field>
        <field name="configurator_done" eval="True" />
        <field name="logo" type="base64" file="furniture_store/static/description/icon.png" />
        <field name="favicon" type="base64" file="furniture_store/static/description/icon.png" />
        <field name="theme_id"
            search="[('id', '=', ref('base.module_theme_loftspace', raise_if_not_found=False))]" />
        <field name="show_line_subtotals_tax_selection">tax_excluded</field>
        <field name="shop_gap">9px</field>
        <field name="product_page_image_width">66_pc</field>
    </record>
</odoo>
