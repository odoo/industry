<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="pos_config_furniture" model="pos.config">
        <field name="name">Furniture Store</field>
        <field name="use_pricelist" eval="True"/>
        <field name="available_pricelist_ids" eval="[(6, 0, [ref('product_pricelist_2')])]"/>
        <field name="pricelist_id" ref="product_pricelist_2"/>
        <field name="ship_later" eval="True"/>
        <field name="down_payment_product_id" ref="pos_sale.default_downpayment_product"/>
    </record>
    <record model="pos.config" id="pos_config_furniture">
        <field name="payment_method_ids" eval="[
            Command.link(ref('pos_payment_method_1')),
            Command.link(ref('pos_payment_method_2')),
        ]"/>
    </record>
</odoo>
