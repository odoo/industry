<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <record id="res_config_settings_enable" model="res.config.settings">
        <field name="group_product_pricelist" eval="1"/>
        <field name="product_pricelist_setting" eval="'advanced'"/>
        <field name="group_product_variant" eval="1"/>
        <field name="group_use_lead" eval="1"/>
        <field name="stock_move_sms_validation" eval="0"/>
        <field name="group_uom" eval="1"/>
        <field name="group_discount_per_so_line" eval="1"/>
        <field name="group_stock_multi_locations" eval="1"/>
        <field name="group_stock_adv_location" eval="1"/>
    </record>
    <function model="res.config.settings" name="execute">
        <value eval="[ref('furniture_store.res_config_settings_enable')]"/>
    </function>
</odoo>
