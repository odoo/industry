<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="planning_model_planning_role_x_is_a_room_offer_field" model="ir.model.fields">
    <field name="ttype">boolean</field>
    <field name="copied" eval="True"/>
    <field name="field_description">Is Stay Offer</field>
    <field name="model_id" ref="planning.model_planning_role"/>
    <field name="name">x_is_a_room_offer</field>
  </record>
  <record id="base_model_res_partner_x_nationality_field" model="ir.model.fields">
    <field name="ttype">many2one</field>
    <field name="copied" eval="True"/>
    <field name="field_description">Nationality</field>
    <field name="model_id" ref="base.model_res_partner"/>
    <field name="name">x_nationality</field>
    <field name="relation">res.country</field>
  </record>
  <record id="base_model_res_partner_x_document_type_field" model="ir.model.fields">
    <field name="ttype">selection</field>
    <field name="copied" eval="True"/>
    <field name="field_description">Document Type</field>
    <field name="model_id" ref="base.model_res_partner"/>
    <field name="name">x_document_type</field>
  </record>
  <record id="base_model_res_partner_x_document_type_field_selection_passport" model="ir.model.fields.selection">
    <field name="name">Passport</field>
    <field name="field_id" ref="base_model_res_partner_x_document_type_field" />
    <field name="value">Passport</field>
  </record>
  <record id="base_model_res_partner_x_document_type_field_selection_id_card" model="ir.model.fields.selection">
    <field name="name">ID card</field>
    <field name="field_id" ref="base_model_res_partner_x_document_type_field" />
    <field name="value">ID card</field>
  </record>
  <record id="base_model_res_partner_x_document_type_field_selection_driving_license" model="ir.model.fields.selection">
    <field name="name">Driving license</field>
    <field name="field_id" ref="base_model_res_partner_x_document_type_field" />
    <field name="value">Driving license</field>
  </record>
  <record id="base_model_res_partner_x_document_number_field" model="ir.model.fields">
    <field name="ttype">char</field>
    <field name="copied" eval="True"/>
    <field name="field_description">Document Number</field>
    <field name="model_id" ref="base.model_res_partner"/>
    <field name="name">x_document_number</field>
  </record>
  <record id="base_model_res_partner_x_identity_check_field" model="ir.model.fields">
    <field name="ttype">selection</field>
    <field name="depends">x_nationality, x_document_type, x_document_number</field>
    <field name="field_description">Identity Check</field>
    <field name="model_id" ref="base.model_res_partner"/>
    <field name="name">x_identity_check</field>
    <field name="readonly" eval="True"/>
    <field name="selectable" eval="False"/>
    <field name="store" eval="False"/>
  </record>
  <record id="base_model_res_partner_x_identity_check_field_selection_na" model="ir.model.fields.selection">
    <field name="name">NA</field>
    <field name="field_id" ref="base_model_res_partner_x_identity_check_field" />
    <field name="value">na</field>
  </record>
  <record id="base_model_res_partner_x_identity_check_field_selection_ok" model="ir.model.fields.selection">
    <field name="name">OK</field>
    <field name="field_id" ref="base_model_res_partner_x_identity_check_field" />
    <field name="value">ok</field>
  </record>
  <record id="base_model_res_partner_x_identity_check_field_selection_invalid" model="ir.model.fields.selection">
    <field name="name">Invalid</field>
    <field name="field_id" ref="base_model_res_partner_x_identity_check_field" />
    <field name="value">invalid</field>
  </record>
  <record id="base_model_res_partner_x_identity_check_field" model="ir.model.fields">
    <field name="compute"><![CDATA[for record in self:
    if not record.x_nationality and not record.x_document_type and not record.x_document_number:
        record['x_identity_check'] = 'na'
    elif record.x_nationality and record.x_document_type and record.x_document_number:
        record['x_identity_check'] = 'ok'
    else: record['x_identity_check'] = 'invalid'
]]></field>
  </record>
  <record id="product_model_product_template_x_is_a_room_offer_field" model="ir.model.fields">
    <field name="ttype">boolean</field>
    <field name="copied" eval="True"/>
    <field name="field_description">Is Stay Offer</field>
    <field name="model_id" ref="product.model_product_template"/>
    <field name="name">x_is_a_room_offer</field>
    </record>
  <record id="product_model_product_template_x_is_stay_tax_field" model="ir.model.fields">
    <field name="ttype">boolean</field>
    <field name="copied" eval="True"/>
    <field name="field_description">Is City Tax</field>
    <field name="model_id" ref="product.model_product_template"/>
    <field name="name">x_is_stay_tax</field>
  </record>
  <record id="product_model_product_template_x_resource_ids_field" model="ir.model.fields">
    <field name="ttype">many2many</field>
    <field name="field_description">Resources</field>
    <field name="model_id" ref="product.model_product_template"/>
    <field name="name">x_resource_ids</field>
    <field name="related">planning_role_id.resource_ids</field>
    <field name="relation">resource.resource</field>
  </record>
  <record id="sale_model_sale_order_x_nights_field" model="ir.model.fields">
    <field name="ttype">integer</field>
    <field name="field_description">Nights</field>
    <field name="readonly" eval="True"/>
    <field name="model_id" ref="sale.model_sale_order_line"/>
    <field name="copied" eval="False"/>
    <field name="name">x_nights</field>
  </record>
  <record id="sale_order_x_order_involves_room" model="ir.model.fields">
    <field name="ttype">boolean</field>
    <field name="field_description">Order Involves Room</field>
    <field name="model_id" ref="sale.model_sale_order"/>
    <field name="name">x_order_involves_room</field>
    <field name="compute"><![CDATA[
for record in self: record['x_order_involves_room'] = any(line.product_id.x_is_a_room_offer for line in record.order_line)
]]></field>
    <field name="depends">order_line</field>
  </record>
    <record id="sale_model_sale_order_x_picked_up" model="ir.model.fields">
    <field name="ttype">boolean</field>
    <field name="field_description">Picked up</field>
    <field name="readonly" eval="True"/>
    <field name="model_id" ref="sale.model_sale_order"/>
    <field name="name">x_picked_up</field>
    <field name="compute"><![CDATA[for record in self:
    record['x_picked_up'] = False
    if record.state == 'sale' and record.is_rental_order:
        rental_order_lines = record.order_line.filtered(lambda line: line.is_rental and line.product_type != 'combo')
        record['x_picked_up'] = not any(sol.qty_delivered < sol.product_uom_qty for sol in rental_order_lines)
]]></field>
    <field name="depends">order_line.qty_delivered</field>
  </record>
  <record id="resource_x_on_going_booking" model="ir.model.fields">
    <field name="ttype">one2many</field>
    <field name="domain">[('sale_line_id.qty_delivered', '&gt;', 0), ('sale_line_id.qty_returned', '=', 0)]</field>
    <field name="field_description">Ongoing Booking</field>
    <field name="model_id" ref="resource.model_resource_resource"/>
    <field name="relation">planning.slot</field>
    <field name="relation_field">resource_id</field>
    <field name="name">x_ongoing_booking</field>
  </record>
  <record id="model_product_attribute_x_captures_guests" model="ir.model.fields">
    <field name="ttype">boolean</field>
    <field name="copied" eval="True"/>
    <field name="field_description">Captures Guests</field>
    <field name="model_id" ref="product.model_product_attribute"/>
    <field name="name">x_captures_guests</field>
  </record>
  <record id="model_product_attribute_value_x_adults" model="ir.model.fields">
    <field name="ttype">integer</field>
    <field name="copied" eval="True"/>
    <field name="field_description">Adults</field>
    <field name="model_id" ref="product.model_product_attribute_value"/>
    <field name="name">x_adults</field>
  </record>
  <record id="model_product_attribute_value_x_children" model="ir.model.fields">
    <field name="ttype">integer</field>
    <field name="copied" eval="True"/>
    <field name="field_description">Children</field>
    <field name="model_id" ref="product.model_product_attribute_value"/>
    <field name="name">x_children</field>
  </record>
  <record id="model_sale_order_line_x_total_guests" model="ir.model.fields">
    <field name="compute"><![CDATA[for record in self:
    x_total_guests = 0
    for product_template_attribute_value in record.product_id.product_template_attribute_value_ids:
        if product_template_attribute_value.attribute_id.x_captures_guests:
            if product_attribute_value := product_template_attribute_value.attribute_id.value_ids.filtered(lambda v: v.id == product_template_attribute_value.product_attribute_value_id.id):
                x_total_guests += product_attribute_value.x_adults + product_attribute_value.x_children
    record['x_total_guests'] = x_total_guests
    ]]></field>
    <field name="ttype">integer</field>
    <field name="depends">product_id</field>
    <field name="field_description">Guests</field>
    <field name="model_id" ref="sale.model_sale_order_line"/>
    <field name="name">x_total_guests</field>
    <field name="readonly" eval="True"/>
    <field name="selectable" eval="False"/>
    <field name="store" eval="False"/>
  </record>
  <record id="model_sale_order_line_x_resource_id" model="ir.model.fields">
    <field name="ttype">many2one</field>
    <field name="related">planning_slot_ids.resource_id</field>
    <field name="field_description">Resource</field>
    <field name="model_id" ref="sale.model_sale_order_line"/>
    <field name="name">x_resource_id</field>
    <field name="readonly" eval="True"/>
    <field name="relation">resource.resource</field>
    <field name="store" eval="False"/>
  </record>
  <record id="resource_x_is_stay_resource" model="ir.model.fields">
    <field name="ttype">boolean</field>
    <field name="field_description">Is Stay Resource</field>
    <field name="model_id" ref="resource.model_resource_resource"/>
    <field name="name">x_is_stay_resource</field>
  </record>
  <record id="model_planning_slot_x_nights" model="ir.model.fields">
    <field name="ttype">integer</field>
    <field name="field_description">Nights</field>
    <field name="model_id" ref="planning.model_planning_slot"/>
    <field name="depends">start_datetime, end_datetime</field>
    <field name="name">x_nights</field>
    <field name="compute"><![CDATA[
for record in self: record['x_nights'] = ((record.end_datetime - record.start_datetime).total_seconds() + 86399) // 86400 if (record.start_datetime and record.end_datetime) else 0
]]></field>
  </record>
  <record id="model_planning_slot_x_guests" model="ir.model.fields">
    <field name="ttype">integer</field>
    <field name="field_description">Guests</field>
    <field name="model_id" ref="planning.model_planning_slot"/>
    <field name="name">x_guests</field>
    <field name="related">sale_line_id.x_total_guests</field>
  </record>
  <record id="model_planning_slot_x_city_tax" model="ir.model.fields">
    <field name="ttype">integer</field>
    <field name="field_description">City tax count</field>
    <field name="model_id" ref="planning.model_planning_slot"/>
    <field name="depends">x_nights, x_guests</field>
    <field name="name">x_city_tax</field>
    <field name="compute"><![CDATA[
for record in self: record['x_city_tax'] = record.x_nights * record.x_guests
]]></field>
  </record>
  <record id="x_model_city_tax_x_sale_order_id" model="ir.model.fields">
    <field name="ttype">many2one</field>
    <field name="field_description">Sale order</field>
    <field name="model_id" ref="x_model_city_tax"/>
    <field name="relation">sale.order</field>
    <field name="name">x_sale_order_id</field>
    <field name="readonly" eval="True"/>
  </record>
  <record id="x_model_city_tax_x_slot_ids" model="ir.model.fields">
    <field name="ttype">many2many</field>
    <field name="field_description">Slots</field>
    <field name="model_id" ref="x_model_city_tax"/>
    <field name="relation">planning.slot</field>
    <field name="name">x_slot_ids</field>
    <field name="depends">x_sale_order_id</field>
    <field name="compute"><![CDATA[
for record in self: record['x_slot_ids'] = record.x_sale_order_id.order_line.planning_slot_ids.ids
      ]]></field>
  </record>
  <record id="x_model_city_tax_x_total" model="ir.model.fields">
    <field name="ttype">integer</field>
    <field name="field_description">Total</field>
    <field name="model_id" ref="x_model_city_tax"/>
    <field name="relation_field">planning_total</field>
    <field name="name">x_total</field>
    <field name="depends">x_slot_ids, x_slot_ids.x_city_tax</field>
    <field name="compute"><![CDATA[
for record in self: record['x_total'] = sum(record.x_slot_ids.mapped('x_city_tax'))
]]></field>
    <field name="readonly" eval="True"/>
    <field name="store" eval="False"/>
  </record>
  <record id="x_model_guests_line_x_sequence" model="ir.model.fields">
    <field name="ttype">integer</field>
    <field name="copied" eval="True"/>
    <field name="field_description">Sequence</field>
    <field name="model_id" ref="x_model_guests_line"/>
    <field name="name">x_sequence</field>
  </record>
  <record id="x_model_guests_line_x_name" model="ir.model.fields">
    <field name="ttype">char</field>
    <field name="copied" eval="True"/>
    <field name="field_description">Description</field>
    <field name="model_id" ref="x_model_guests_line"/>
    <field name="name">x_name</field>
    <field name="translate">standard</field>
  </record>
  <record id="x_model_guests_line_x_sale_order_id" model="ir.model.fields">
    <field name="ttype">many2one</field>
    <field name="copied" eval="True"/>
    <field name="field_description">Sale Order</field>
    <field name="model_id" ref="x_model_guests_line"/>
    <field name="name">x_sale_order_id</field>
    <field name="relation">sale.order</field>
  </record>
  <record id="x_model_guests_line_x_guest_partner_id" model="ir.model.fields">
    <field name="ttype">many2one</field>
    <field name="copied" eval="True"/>
    <field name="field_description">Guest</field>
    <field name="model_id" ref="x_model_guests_line"/>
    <field name="name">x_guest_partner_id</field>
    <field name="relation">res.partner</field>
  </record>
  <record id="x_model_guests_line_x_room_resource_id" model="ir.model.fields">
    <field name="ttype">many2one</field>
    <field name="copied" eval="True"/>
    <field name="field_description">Room</field>
    <field name="model_id" ref="x_model_guests_line"/>
    <field name="name">x_room_resource_id</field>
    <field name="relation">resource.resource</field>
  </record>
  <record id="x_model_guests_line_x_sol_resource_ids" model="ir.model.fields">
    <field name="ttype">many2many</field>
    <field name="field_description">Resource IDs</field>
    <field name="model_id" ref="x_model_guests_line"/>
    <field name="name">x_sol_resource_ids</field>
    <field name="relation">resource.resource</field>
    <field name="depends">x_sale_order_id.order_line</field>
    <field name="store" eval="False"/>
    <field name="compute"><![CDATA[ 
for record in self: record['x_sol_resource_ids'] = record.x_sale_order_id.mapped('order_line.x_resource_id.id')
      ]]></field>
  </record>
  <record id="x_model_guests_line_x_guest_identity_check" model="ir.model.fields">
    <field name="ttype">selection</field>
    <field name="related">x_guest_partner_id.x_identity_check</field>
    <field name="field_description">Identity Check</field>
    <field name="model_id" ref="x_model_guests_line"/>
    <field name="name">x_guest_identity_check</field>
    <field name="readonly" eval="True"/>
    <field name="selectable" eval="False"/>
    <field name="store" eval="False"/>
  </record>
  <record id="model_sale_order_x_guest_line_ids" model="ir.model.fields">
    <field name="ttype">one2many</field>
    <field name="field_description">Guest Line</field>
    <field name="model_id" ref="sale.model_sale_order"/>
    <field name="name">x_guest_line_ids</field>
    <field name="relation">x_guests_line</field>
    <field name="relation_field">x_sale_order_id</field>
  </record>
  <record id="sale_model_sale_order_x_guests_field" model="ir.model.fields">
    <field name="ttype">many2many</field>
    <field name="copied" eval="True"/>
    <field name="field_description">Guests</field>
    <field name="model_id" ref="sale.model_sale_order"/>
    <field name="name">x_guests</field>
    <field name="relation">res.partner</field>
    <field name="relation_table">x_res_partner_sale_order_rel</field>
    <field name="depends">x_guest_line_ids</field>
    <field name="compute"><![CDATA[
for record in self:  record['x_guests'] = [(6, 0, record.x_guest_line_ids.mapped('x_guest_partner_id').ids)]
      ]]></field>
  </record>
  <record id="rental_order_wizard_x_resource_id" model="ir.model.fields">
    <field name="ttype">many2one</field>
    <field name="field_description">Resource</field>
    <field name="model_id" ref="sale_renting.model_rental_order_wizard_line"/>
    <field name="name">x_resource_id</field>
    <field name="relation">resource.resource</field>
    <field name="related">order_line_id.x_resource_id</field>
  </record>
  <record id="resource_x_occupant_ids" model="ir.model.fields">
    <field name="name">x_occupant_ids</field>
    <field name="field_description">Occupant</field>
    <field name="ttype">many2many</field>
    <field name="model_id" ref="resource.model_resource_resource"/>
    <field name="relation">res.partner</field>
    <field name="readonly" eval="True"/>
    <field name="depends">x_ongoing_booking.sale_order_id.x_guest_line_ids, x_ongoing_booking</field>
    <field name="compute"><![CDATA[
for record in self: record['x_occupant_ids'] = [(6, 0, record.x_ongoing_booking[:1].sale_order_id.x_guest_line_ids.filtered(lambda l: l.x_room_resource_id.id == record.id).mapped('x_guest_partner_id').ids)] if record.x_ongoing_booking else [(5, 0, 0)]
  ]]></field>
  </record>
  <record id="res_partner_x_ongoing_bookings" model="ir.model.fields">
    <field name="ttype">many2many</field>
    <field name="field_description">Current Resource</field>
    <field name="model_id" ref="base.model_res_partner"/>
    <field name="relation">resource.resource</field>
    <field name="readonly" eval="True"/>
    <field name="name">x_ongoing_bookings</field>
    <field name="compute"><![CDATA[ 
for record in self: record['x_ongoing_bookings'] = self.env['resource.resource'].search([('x_occupant_ids', 'in', record.id)]).ids
    ]]></field>
    <field name="store" eval="False"/>
  </record>
  <record id="res_partner_x_guests_count" model="ir.model.fields">
    <field name="compute"><![CDATA[
for record in self: record['x_guests_count'] = self.env['x_guests_line'].search_count([('x_guest_partner_id.id', '=', record.id)])
]]></field>
    <field name="ttype">integer</field>
    <field name="field_description">Guests count</field>
    <field name="model_id" ref="base.model_res_partner"/>
    <field name="selectable" eval="False"/>
    <field name="store" eval="False"/>
    <field name="readonly" eval="True"/>
    <field name="name">x_guests_count</field>
  </record>
</odoo>
