<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <function name="button_choose_theme" model="ir.module.module" eval="[ref('base.module_theme_artists', raise_if_not_found=False) or ref('base.module_theme_default')]"/>

  <function model="web_editor.assets" name="make_scss_customization" context="{'website_id': ref('website.default_website')}">
    <value eval="'/website/static/src/scss/options/colors/user_color_palette.scss'"/>
    <value eval="{'menu': 2}"/>
  </function>

  <function model="web_editor.assets" name="make_scss_customization">
    <value eval="'/website/static/src/scss/options/user_values.scss'"/>
    <value eval="{
      'menu-gradient': 'null',
      'menu-secondary-gradient': 'null',
      'footer-gradient': 'null',
      'copyright-gradient': 'null',
      'o-cc1-bg-gradient': 'null',
      'o-cc2-bg-gradient': 'null',
      'o-cc3-bg-gradient': 'null',
      'o-cc4-bg-gradient': 'null',
      'o-cc5-bg-gradient': 'null',
      'font': 'null',
      'headings-font': 'null',
      'navbar-font': 'Oxygen',
      'buttons-font': 'null',
      'color-palettes-number': 'null',
      'color-palettes-name': 'default-2',
      'btn-ripple': 'null',
      'header-template': 'sidebar',
      'footer-template': 'null',
      'footer-scrolltop': 'null',
      'header-links-style': 'default',
      'sidebar-width': '15.625rem',
      'header-text-color': 'var(--o-color-5)',
      'menu-box-shadow': 'var(--o-color-1) 0px 1px 3px 0px',
      'menu-border-radius': '0px',
      'header-font-size': '1rem',
      'font-size-base': '1rem',
    }"/>
  </function>

  <function model="web_editor.assets" name="make_scss_customization">
    <value eval="'/website/static/src/scss/options/colors/user_theme_color_palette.scss'"/>
    <value eval="{
        'success': 'null',
        'info': 'null',
        'warning': 'null',
        'danger': 'null',

      }"/>
  </function>

  <function model="ir.ui.view" name="write">
    <value model="ir.ui.view" eval="obj().env['website'].with_context(website_id=obj().env.ref('website.default_website').id).viewref('website.homepage').id"/>
    <value model="ir.ui.view" eval="{'arch': obj().env.ref('coworking.homepage').arch.replace(
        '*free_tour_appointment_type*', str(obj().env.ref('coworking.appointment_type_2').id)).replace(
        '*one_day_pass_product*', str(obj().env.ref('coworking.product_product_11').website_url)).replace(
        '*10_day_pass_card_product*', str(obj().env.ref('coworking.product_product_10').website_url))
    }"/>
  </function>

  <function name="write" model="ir.ui.view">
    <value model="ir.ui.view" eval="obj().env['website'].with_context(website_id=ref('website.default_website')).viewref('website.contactus').id"/>
    <value model="ir.ui.view" eval="{'arch': obj().env.ref('coworking.ir_ui_view_4210').arch}"/>
  </function>

  <function model="ir.ui.view" name="write">
    <value model="ir.ui.view" eval="obj().env['website'].with_context(website_id=obj().env.ref('website.default_website').id).viewref('website.template_footer_centered').id"/>
    <value model="ir.ui.view" eval="{'arch': obj().env.ref('coworking.ir_ui_view_3303').arch}"/>
  </function>

  <function model="ir.ui.view" name="write">
    <value model="ir.ui.view" eval="obj().env.ref('coworking.ir_ui_view_3304').id"/>
    <value model="ir.ui.view" eval="{'arch': obj().env.ref('coworking.ir_ui_view_3304').arch.replace(
        '*shared_desk_appointment_type*', str(obj().env.ref('coworking.appointment_type_1').id))
    }"/>
  </function>

  <function model="ir.ui.view" name="write">
    <value model="ir.ui.view" eval="obj().env.ref('coworking.ir_ui_view_4092').id"/>
    <value model="ir.ui.view" eval="{'arch': obj().env.ref('coworking.ir_ui_view_4092').arch.replace(
        '*shared_desk_appointment_type*', str(obj().env.ref('coworking.appointment_type_1').id)).replace(
        '*meeting_room_appointment_type*', str(obj().env.ref('coworking.appointment_type_3').id))
    }"/>
  </function>
</odoo>
