<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
  <record id="website.default_website" model="website" forcecreate="1">
    <field name="name">Textile Manufacturing</field>
    <field name="configurator_done" eval="True"/>
    <field name="logo" type="base64" file="textile_manufacturing/static/description/icon.png"/>
    <field name="favicon" type="base64" file="textile_manufacturing/static/description/icon.png"/>
    <field name="user_id" ref="base.public_user"/>
    <field name="theme_id" search="[('id', '=', ref('base.module_theme_anelusia', raise_if_not_found=False))]"/>
    <field name="salesperson_id" ref="base.user_admin"/>
    <field name="show_line_subtotals_tax_selection">tax_excluded</field>
    <field name="cart_recovery_mail_template_id" eval="False"/>
    <field name="shop_ppr">5</field>
    <field name="shop_gap">0px</field>
    <field name="prevent_zero_price_sale" eval="True"/>
  </record>
</odoo>
