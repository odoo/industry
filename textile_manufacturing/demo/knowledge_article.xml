  <record id="knowledge_article_44" model="knowledge.article">
    <field name="name">S00001</field>
    <field name="body"><![CDATA[
        <h1>Corporate Shirt</h1>
        <p><br /></p>
        <h2>Design</h2>
        <p><br /></p>
        <p><img src="https://textile-manufacturing-indus.odoo.com/web/image/1307-9d664dac/image.png?access_token=ecf9ebc2-fc4b-4b76-8c4c-b75752312b5f" data-file-name="image.png" class="img-fluid" /></p>
        <p><br /></p>
        <h2>Measure:</h2>
        <p><br /></p>
        <p><img src="https://textile-manufacturing-indus.odoo.com/web/image/1308-d052c0f0/image.png?access_token=1e036e0a-7244-43fe-8af0-91ff4ee9dc1c" data-file-name="image.png" class="img-fluid" /></p>
        <p><br /></p>
        <ul>
            <li>
                <p><b>A - Body Length (HPS to Hem):</b> 76 cm</p>
            </li>
            <li>
                <p><b>B - Chest Width (1" below armhole):</b> 54 cm</p>
            </li>
            <li>
                <p><b>C - Waist Width (15" below HPS):</b> 51 cm</p>
            </li>
            <li>
                <p><b>D - Bottom Hem Width:</b> 54 cm</p>
            </li>
            <li>
                <p><b>E - Shoulder Width (Point to Point):</b> 46 cm</p>
            </li>
            <li>
                <p><b>F - Sleeve Length (Shoulder to Cuff Hem):</b> 66 cm</p>
            </li>
            <li>
                <p><b>G - Cuff Height:</b> 7 cm</p>
            </li>
            <li>
                <p><b>H - Collar Height (at CB):</b> 4 cm</p>
            </li>
            <li>
                <p><b>I - Collar Point Length:</b> 7.5 cm</p>
            </li>
        </ul>
        <h4><br /></h4>
        <p><br /></p>
        <h2>Component for BOM</h2>
        <table style="width: 700px; margin-left: 6px;" class="table table-bordered">
            <tbody>
                <tr>
                    <td style="width: 138px;">Component Name</td>
                    <td style="width: 139.594px;">Quantity</td>
                    <td style="width: 141.797px;">Unit of Measure</td>
                    <td style="width: 141.797px;">Applied For Variant</td>
                    <td style="width: 137.812px;">Notes</td>
                </tr>
                <tr>
                    <td style="width: 141.797px;"><p>Dyed Cotton Fabric Blue</p></td>
                    <td style="width: 141.797px;">1.5</td>
                    <td style="width: 141.797px;">meters (m)</td>
                    <td style="width: 141.797px;">Blue</td>
                    <td style="width: 141.812px;"><p>Weight: 120 GSM, Width: 150 cm</p></td>
                </tr>
                <tr>
                    <td style="width: 141.797px;">Interfacing (Woven)</td>
                    <td style="width: 141.797px;">0.25</td>
                    <td style="width: 141.797px;">meters (m)</td>
                    <td style="width: 141.797px;">All</td>
                    <td style="width: 141.812px;"><p>For collar, cuffs, and placket. Weight: 100 GSM</p></td>
                </tr>
                <tr>
                    <td style="width: 141.797px;"><p>Sewing thread</p></td>
                    <td style="width: 141.797px;">2.3</td>
                    <td style="width: 141.797px;">meters (m)</td>
                    <td style="width: 141.797px;">All</td>
                    <td style="width: 141.812px;"><p>Color: Light Blue, Tex 27, Qty per shirt</p></td>
                </tr>
                <tr>
                    <td style="width: 141.797px;">Plastic Buttons (11.4mm)</td>
                    <td style="width: 141.797px;">9</td>
                    <td style="width: 141.797px;">unit (pc)</td>
                    <td style="width: 141.797px;">All</td>
                    <td style="width: 141.812px;"><p>7 for front, 2 for cuffs</p></td>
                </tr>
                <tr>
                    <td style="width: 141.797px;">Plastic Buttons (9.5mm)</td>
                    <td style="width: 141.797px;">4</td>
                    <td style="width: 141.797px;">unit (pc)</td>
                    <td style="width: 141.797px;">All</td>
                    <td style="width: 141.812px;"><p>2 for sleeve placket, 2 for collar</p></td>
                </tr>
                <tr>
                    <td style="width: 141.797px;"><p>Clothing Label (Top)</p></td>
                    <td style="width: 141.797px;">1</td>
                    <td style="width: 141.797px;">unit (pc)</td>
                    <td style="width: 141.797px;">All</td>
                    <td style="width: 141.812px;"><p>"MyCo" logo, 2x5 cm</p></td>
                </tr>
            </tbody>
        </table>
        <p><br /></p>
    ]]></field>
    <field name="parent_id" ref="knowledge_article_37"/>
    <field name="is_locked" eval="True"/>
  </record>
