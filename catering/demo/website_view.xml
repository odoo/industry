<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="homepage" model="ir.ui.view">
    <field name="arch" type="xml">
      <t name="Homepage" t-name="website.homepage">
        <t t-call="website.layout" pageName.f="homepage">
          <div id="wrap" class="oe_structure">
            <section class="s_discovery pt136 pb136 o_cc o_cc5 o_colored_level" data-snippet="s_discovery">
              <div style="text-align: center;" class="container">
                <h1><strong><b>The art of making your special moments shine.</b>&amp;nbsp;</strong>&amp;nbsp;</h1>
                <p>Your catering service, transforming every occasion, from intimate dinners to grand corporate events and fairytale weddings,&amp;nbsp;</p>
                <p>into a masterpiece of flavor and style.</p>
                <p style="text-align: center;">
                  <a class="btn btn-lg btn-primary o_translate_inline" href="/contactus">Contact Us</a>
                </p>
              </div>
            </section>
            <section class="s_parallax parallax s_parallax_is_fixed bg-black-50 pt48 pb48 o_colored_level o_half_screen_height" data-scroll-background-ratio="3" data-snippet="s_parallax" style="position: relative;" data-oe-shape-data="{&quot;shape&quot;:&quot;html_builder/Connections/01&quot;,&quot;flip&quot;:[],&quot;showOnMobile&quot;:false,&quot;shapeAnimationSpeed&quot;:&quot;0&quot;}">
