<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="website.default_website" model="website" forcecreate="1">
        <field name="name">Nightclubs</field>
        <field name="configurator_done" eval="True" />
        <field name="user_id" ref="base.public_user" />
        <field name="theme_id" search="[('id', '=', ref('base.module_theme_bistro', raise_if_not_found=False))]"/>
        <field name="salesperson_id" ref="base.user_admin" />
        <field name="logo" type="base64" file="night_clubs/static/description/icon.png"/>
        <field name="favicon" type="base64" file="night_clubs/static/description/icon.png"/>
    </record>
</odoo>
