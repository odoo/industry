<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <function model="pos.config" name="load_onboarding_bar_scenario" />
    <record id="pos_restaurant.pos_config_main_bar" model="pos.config" forcecreate="1">
        <field name="name">Bar</field>
        <field name="limit_categories" eval="True" />
        <field name="iface_available_categ_ids" eval="[(6, 0, [
			ref('pos_restaurant.pos_category_cocktails', raise_if_not_found=False),
			ref('pos_restaurant.pos_category_soft_drinks', raise_if_not_found=False)
		])]"/>
        <field name="tip_product_id" eval="False" />
        <field name="show_product_images" eval="False" />
        <field name="show_category_images" eval="False" />
        <field name="default_screen">register</field>
    </record>
    <record id="pos_config_2" model="pos.config">
        <field name="name">Ticket Shop</field>
        <field name="limit_categories" eval="True"/>
        <field name="iface_available_categ_ids" eval="[(6, 0, [ref('pos_category_4')])]"/>
        <field name="tip_product_id" eval="False"/>
    </record>
</odoo>
