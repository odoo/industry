    <field name="selection">[('Installation', 'Installation'), ('Maintenance', 'Maintenance'), ('Repair', 'Repair')]</field>
  </record>
  <record id="ir_model_fields_13808" model="ir.model.fields">
    <field name="ttype">selection</field>
    <field name="copied" eval="True"/>
    <field name="field_description">Primary Root Cause</field>
    <field name="model">x_project_task_worksheet_template_2</field>
    <field name="model_id" ref="industry_fsm_report.x_project_task_worksheet_template_2"/>
    <field name="name">x_primary_root_cause</field>
    <field name="selection">[('Misuse', 'Misuse'), ('Defect', 'Defect'), ('Natural Failure', 'Natural Failure'), ('Unknown', 'Unknown')]</field>
  </record>
  <record id="ir_model_fields_13806" model="ir.model.fields">
    <field name="ttype">boolean</field>
    <field name="copied" eval="True"/>
    <field name="field_description">Product is functional after operation</field>
    <field name="model">x_project_task_worksheet_template_2</field>
    <field name="model_id" ref="industry_fsm_report.x_project_task_worksheet_template_2"/>
