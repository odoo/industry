    </record>
  
  <record id="ir_model_fields_13802" model="ir.model.fields">
    <field name="ttype">float</field>
    <field name="copied" eval="True"/>
    <field name="field_description">Removed Refrigerant</field>
    <field name="model">x_project_task_worksheet_template_2</field>
    <field name="model_id" ref="industry_fsm_report.x_project_task_worksheet_template_2"/>
    <field name="name">x_removed_refrigerant</field>
    </record>
  <record id="ir_model_fields_13800" model="ir.model.fields">
    <field name="ttype">html</field>
    <field name="copied" eval="True"/>
    <field name="field_description">Root Cause Comments</field>
    <field name="model">x_project_task_worksheet_template_2</field>
    <field name="model_id" ref="industry_fsm_report.x_project_task_worksheet_template_2"/>
    <field name="name">x_root_cause_comments</field>
    </record>
  <record id="ir_model_fields_13798" model="ir.model.fields">
    <field name="ttype">binary</field>
    <field name="copied" eval="True"/>
    <field name="field_description">Worker Signature</field>
    <field name="model">x_project_task_worksheet_template_2</field>
    <field name="model_id" ref="industry_fsm_report.x_project_task_worksheet_template_2"/>
