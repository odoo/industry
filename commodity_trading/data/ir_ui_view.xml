<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <record id="report_custom_x_inward_checking_worksheet" model="ir.ui.view">
        <field name="name">x_inward_checking_worksheet</field>
        <field name="key">gen_key.500ba5</field>
        <field name="type">qweb</field>
        <field name="arch" type="xml">
            <t t-name="x_inward_checking_worksheet">
                <div>
                    <div class="row" style="page-break-inside: avoid">
                        <div class="col-6" style="page-break-inside: avoid">
                            <div class="row mb-3" style="page-break-inside: avoid">
                                <div t-att-class="('col-5' if report_type == 'pdf' else 'col-lg-5 col-12') + ' font-weight-bold'">Moisture(%)</div>
                                <div help="8% to 8.5% Maximum." string="Moisture(%)" t-att-class="'col-7' if report_type == 'pdf' else 'col-lg-7 col-12'" t-field="worksheet.x_moisture"/>
                            </div>
                            <div class="row mb-3" style="page-break-inside: avoid">
                                <div t-att-class="('col-5' if report_type == 'pdf' else 'col-lg-5 col-12') + ' font-weight-bold'">Split Broken(%)</div>
                                <div string="Split Broken(%)" help="0.5% Maximum" t-att-class="'col-7' if report_type == 'pdf' else 'col-lg-7 col-12'" t-field="worksheet.x_split_broken"/>
                            </div>
                        </div>
                        <div class="col-6" style="page-break-inside: avoid">
                            <div class="row mb-3" style="page-break-inside: avoid">
                                <div t-att-class="('col-5' if report_type == 'pdf' else 'col-lg-5 col-12') + ' font-weight-bold'">Admixture(%)</div>
                                <div string="Admixture(%)" t-att-class="'col-7' if report_type == 'pdf' else 'col-lg-7 col-12'" t-field="worksheet.x_admixture"/>
                            </div>
                            <div class="row mb-3" style="page-break-inside: avoid">
                                <div t-att-class="('col-5' if report_type == 'pdf' else 'col-lg-5 col-12') + ' font-weight-bold'">Damage(%)</div>
                                <div string="Damage(%)" help="0.5% Maximum" t-att-class="'col-7' if report_type == 'pdf' else 'col-lg-7 col-12'" t-field="worksheet.x_damage"/>
                            </div>
                        </div>
                    </div>
                </div>
            </t>
        </field>
    </record>

    <record id="x_weighing_scale_maste_form_view" model="ir.ui.view">
        <field name="name">Default form view for x_weighing_scale_maste</field>
        <field name="model">x_weighing_scale_maste</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form>
                <sheet string="Weighing Scale Master">
                    <widget name="web_ribbon" text="Archived" bg_color="text-bg-danger" invisible="x_active == True"/>
                    <field name="x_active" invisible="1"/>
                    <div class="oe_title">
                        <h1>
                            <field name="x_name" required="1" placeholder="Name..."/>
                        </h1>
                    </div>
                    <group name="group_weighing_scale"/>
                </sheet>
                <div class="oe_chatter" name="oe_chatter">
                    <field name="message_follower_ids"/>
                    <field name="message_ids"/>
                    <field name="activity_ids"/>
                </div>
            </form>
        </field>
    </record>
    <record id="x_weighing_scale_maste_form_view_inherit" model="ir.ui.view">
        <field name="name">Default form view for x_weighing_scale_maste customization</field>
        <field name="mode">extension</field>
        <field name="model">x_weighing_scale_maste</field>
        <field name="inherit_id" ref="x_weighing_scale_maste_form_view"/>
        <field name="priority">99</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <data>
                <xpath expr="//group[@name='group_weighing_scale']" position="replace">
                    <group>
                        <group>
                            <field name="x_warehouse" options="{'no_create': True}"/>
                            <xpath expr="//field[@name='x_name']" position="move"/>
                        </group>
                    </group>
                </xpath>
                <xpath expr="//field[@name='x_name']" position="attributes">
                    <attribute name="placeholder"/>
                    <attribute name="required"/>
                </xpath>
            </data>
        </field>
    </record>

