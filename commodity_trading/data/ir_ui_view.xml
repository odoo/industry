    <!-- TODO: check this view is really required -->
    <record id="product_template_form_customization_commodity_trading" model="ir.ui.view">
        <field name="name">product.template.product.form.customization.commodity.trading</field>
        <field name="mode">extension</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"/>
        <field name="priority">160</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <data>
                <!-- TODO: Need to shorter xpath -->
                <xpath expr="/form[1]/sheet[1]/notebook[1]/page[5]/group[2]/field[1]" position="inside">
                    <tree string="Product Packagings" name="packaging" editable="bottom">
                        <field name="sequence" widget="handle"/>
                        <field name="name"/>
                        <field name="package_type_id" groups="stock.group_tracking_lot"/>
                        <field name="route_ids" groups="stock.group_adv_location" optional="hide" widget="many2many_tags"/>
                        <field name="qty"/>
                        <field name="product_uom_id" options="{'no_open': True, 'no_create': True}" groups="uom.group_uom"/>
                        <field name="purchase" optional="show"/>
                        <field name="sales" optional="show"/>
                        <field name="barcode" optional="hide"/>
                        <field name="company_id" groups="base.group_multi_company" optional="hide"/>
                    </tree>
                    <form string="Product Packaging">
                        <sheet>
                            <label for="name"/>
                            <h1>
                                <field name="name"/>
                            </h1>
                            <group>
                                <field name="id" invisible="1"/>
                                <field name="company_id" invisible="1"/>
                                <group name="group_product">
                                    <field name="product_id" readonly="id == False"/>
                                    <field name="sales"/>
                                    <field name="purchase"/>
                                </group>
                                <group name="qty">
                                    <field name="package_type_id" groups="stock.group_tracking_lot"/>
                                    <label for="qty" string="Contained quantity"/>
                                    <div class="o_row">
                                        <field name="qty"/>
                                        <field name="product_uom_id" options="{'no_open': True, 'no_create': True}" groups="uom.group_uom"/>
                                    </div>
                                    <field name="barcode"/>
                                    <field name="company_id" groups="base.group_multi_company"/>
                                </group>
                                <group name="logistic">
                                    <field name="route_ids" groups="stock.group_adv_location" widget="many2many_tags"/>
                                </group>
                            </group>
                        </sheet>
                    </form>
                </xpath>
            </data>
        </field>
    </record>


