<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="stock_picking_1" model="stock.picking">
        <field name="partner_id" ref="base.partner_admin"/>
        <field name="picking_type_id" ref="stock.picking_type_internal"/>
        <field name="location_id" ref="stock.stock_location_stock"/>
        <field name="location_dest_id" search="[('display_name', '=', 'Bar/Stock')]"/>
    </record>
    <record id="stock_move_1" model="stock.move">
        <field name="inventory_name">Move 1</field>
        <field name="product_id" ref="product_product_17"/>
        <field name="product_uom_qty" eval="2"/>
        <field name="product_uom" ref="uom_uom_28"/>
        <field name="picking_id" ref="stock_picking_1"/>
    </record>
    <record id="stock_picking_2" model="stock.picking">
        <field name="partner_id" ref="base.partner_admin"/>
        <field name="picking_type_id" ref="stock.picking_type_internal"/>
        <field name="location_id" ref="stock.stock_location_stock"/>
        <field name="location_dest_id" search="[('display_name', '=', 'Bar/Stock')]"/>
    </record>
    <record id="stock_move_2" model="stock.move">
        <field name="inventory_name">Move 2</field>
        <field name="product_id" ref="product_product_16"/>
        <field name="product_uom_qty" eval="2"/>
        <field name="picking_id" ref="stock_picking_2"/>
    </record>
    <record id="stock_picking_3" model="stock.picking">
        <field name="partner_id" ref="base.partner_admin"/>
        <field name="picking_type_id" ref="stock.picking_type_internal"/>
        <field name="location_id" ref="stock.stock_location_stock"/>
        <field name="location_dest_id" search="[('display_name', '=', 'Bar/Stock')]"/>
    </record>
    <record id="stock_move_3" model="stock.move">
        <field name="inventory_name">Move 3</field>
        <field name="product_id" ref="product_product_15"/>
        <field name="product_uom_qty" eval="2"/>
        <field name="product_uom" ref="uom_uom_34"/>
        <field name="picking_id" ref="stock_picking_3"/>
    </record>
    <record id="stock_picking_4" model="stock.picking">
        <field name="partner_id" ref="base.partner_admin"/>
        <field name="picking_type_id" ref="stock.picking_type_internal"/>
        <field name="location_id" ref="stock.stock_location_stock"/>
        <field name="location_dest_id" search="[('display_name', '=', 'Bar/Stock')]"/>
    </record>
    <record id="stock_move_4" model="stock.move">
        <field name="inventory_name">Move 4</field>
        <field name="product_id" ref="product_product_3"/>
