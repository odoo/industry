<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <function model="pos.config" name="load_onboarding_retail_scenario">
        <value name="with_demo_data" model="ir.module.module" eval="obj().search([('name', '=', 'fmcg_store')]).demo"/>
    </function>
    <record id="point_of_sale.pos_config_retail" model="pos.config" forcecreate="1">
        <field name="name">Grocery Shop</field>
        <field name="is_margins_costs_accessible_to_every_user" eval="True"/>
        <field name="iface_print_auto" eval="True"/>
        <field name="ship_later" eval="True"/>
    </record>
</odoo>
