<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <function model="pos.config" name="load_onboarding_restaurant_scenario">
        <value name="with_demo_data" model="ir.module.module" eval="obj().search([('name', '=', 'fast_food')]).demo"/>
    </function>
    <function model="pos.config" name="load_onboarding_kiosk_scenario"/>

    <function model="pos.config" name="write">
        <value model="pos.config" eval="obj().search([('name', '=', 'Kiosk')], limit=1).id"/>
        <value eval="{'receipt_footer': 'Thank You! Visit Again.'}"/>
    </function>
</odoo>
