<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
  <record id="loyalty_generate_wizard_1" model="loyalty.generate.wizard">
    <field name="program_id" eval="ref('loyalty_program_3')"/>
    <field name="mode">anonymous</field>
    <field name="coupon_qty">50</field>
  </record>
  <record id="loyalty_generate_wizard_2" model="loyalty.generate.wizard">
    <field name="program_id" eval="ref('loyalty_program_2')"/>
    <field name="mode">selected</field>
    <field name="coupon_qty">50</field>
    <field name="customer_ids" eval="[[(6, 0, ref('base_industry_data.res_partner_25'))]]"/>
  </record>
  <function name="generate_coupons" model="loyalty.generate.wizard" eval="[[ref('loyalty_generate_wizard_1')]]"/>
  <function name="generate_coupons" model="loyalty.generate.wizard" eval="[[ref('loyalty_generate_wizard_2')]]"/>
</odoo>
