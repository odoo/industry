<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
  <record id="website.default_website" model="website" forcecreate="1">
    <field name="name">Driving-School</field>
    <field name="configurator_done" eval="True"/>
    <field name="logo" type="base64" file="driving_school/static/description/icon.png"/>
    <field name="favicon" type="base64" file="driving_school/static/description/icon.png"/>
    <field name="user_id" ref="base.public_user"/>
    <field name="theme_id" search="[('id', '=', ref('base.module_theme_loftspace', raise_if_not_found=False))]"/>
    <field name="salesperson_id" ref="base.user_admin"/>
    <field name="show_line_subtotals_tax_selection">tax_included</field>
    <field name="account_on_checkout">mandatory</field>
    <field name="cart_recovery_mail_template_id" eval="False"/>
  </record>
</odoo>
