<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <function name="button_choose_theme" model="ir.module.module"
        eval="[ref('base.module_theme_zap', raise_if_not_found=False) or ref('base.module_theme_default')]"/>

    <function model="ir.ui.view" name="write">
        <value model="ir.ui.view"
            eval="obj().env['website'].with_context(website_id=obj().env.ref('website.default_website').id).viewref('website.template_footer_headline').id"/>
        <value model="ir.ui.view" eval="{'arch': obj().env.ref('ngo.ir_ui_view_4278').arch}"/>
    </function>
    <function model="ir.ui.view" name="write">
        <value model="ir.ui.view"
            eval="obj().env['website'].with_context(website_id=obj().env.ref('website.default_website').id).viewref('website.homepage').id"/>
        <value model="ir.ui.view" eval="{'arch': obj().env.ref('ngo.ir_ui_view_3302').arch}"/>
    </function>
</odoo>
