<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="survey_question_1" model="survey.question">
        <field name="title">General Experience</field>
        <field name="is_page" eval="True"/>
        <field name="survey_id" ref="survey_survey_1"/>
    </record>
    <record id="survey_question_2" model="survey.question">
        <field name="title">How would you rate the overall event?</field>
        <field name="question_type">scale</field>
        <field name="survey_id" ref="survey_survey_1"/>
        <field name="sequence">11</field>
        <field name="scale_min">1</field>
        <field name="scale_max">5</field>
        <field name="scale_min_label">Poor</field>
        <field name="scale_mid_label">Good</field>
        <field name="scale_max_label">Excellent</field>
    </record>
    <record id="survey_question_3" model="survey.question">
        <field name="title">Was the event easy to access (location, timing, info)?</field>
        <field name="question_type">simple_choice</field>
        <field name="survey_id" ref="survey_survey_1"/>
        <field name="sequence">12</field>
    </record>
    <record id="survey_question_4" model="survey.question">
        <field name="title">How did you hear about this event?</field>
        <field name="question_type">simple_choice</field>
        <field name="survey_id" ref="survey_survey_1"/>
        <field name="sequence">13</field>
    </record>
    <record id="survey_question_5" model="survey.question">
        <field name="title">Did the event meet your expectations?</field>
        <field name="question_type">simple_choice</field>
        <field name="survey_id" ref="survey_survey_1"/>
        <field name="sequence">14</field>
    </record>
    <record id="survey_question_6" model="survey.question">
        <field name="title">Participation &amp; Engagement</field>
        <field name="is_page" eval="True"/>
        <field name="survey_id" ref="survey_survey_1"/>
        <field name="sequence">15</field>
    </record>
    <record id="survey_question_7" model="survey.question">
        <field name="title">How engaging were the activities or presentations?</field>
        <field name="question_type">scale</field>
        <field name="survey_id" ref="survey_survey_1"/>
        <field name="sequence">16</field>
        <field name="scale_min">1</field>
        <field name="scale_max">5</field>
    </record>
    <record id="survey_question_8" model="survey.question">
        <field name="title">Did you feel included and welcomed during the event?</field>
        <field name="question_type">simple_choice</field>
        <field name="survey_id" ref="survey_survey_1"/>
        <field name="sequence">17</field>
    </record>
    <record id="survey_question_9" model="survey.question">
        <field name="title">What was your favorite part of the event?</field>
        <field name="question_type">char_box</field>
        <field name="survey_id" ref="survey_survey_1"/>
        <field name="sequence">18</field>
    </record>
    <record id="survey_question_10" model="survey.question">
        <field name="title">What would you suggest to improve future events?</field>
        <field name="question_type">text_box</field>
        <field name="survey_id" ref="survey_survey_1"/>
        <field name="sequence">19</field>
    </record>
    <record id="survey_question_11" model="survey.question">
        <field name="title">Future Interest</field>
        <field name="is_page" eval="True"/>
        <field name="survey_id" ref="survey_survey_1"/>
        <field name="sequence">20</field>
    </record>
    <record id="survey_question_12" model="survey.question">
        <field name="title">Would you attend another ONG event in the future?</field>
        <field name="question_type">simple_choice</field>
        <field name="survey_id" ref="survey_survey_1"/>
        <field name="sequence">21</field>
    </record>
    <record id="survey_question_13" model="survey.question">
        <field name="title">Would you like to receive updates or volunteer opportunities from us?</field>
        <field name="question_type">simple_choice</field>
        <field name="survey_id" ref="survey_survey_1"/>
        <field name="sequence">22</field>
    </record>
</odoo>
