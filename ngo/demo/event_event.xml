<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="event_event_1" model="event.event">
        <field name="name">Clean River Day</field>
        <field name="stage_id" ref="event.event_stage_new"/>
        <field name="seats_max">0</field>
        <field name="user_id" ref="base.user_admin"/>
        <field name="seats_limited" eval="True"/>
        <field name="date_begin" eval="datetime.today().date() + relativedelta(months=1, hours=8)"/>
        <field name="date_end" eval="datetime.today().date() + relativedelta(months=1, hours=18)"/>
    </record>
    <record id="event_event_2" model="event.event">
        <field name="name">Local Mobilization &amp; Petition Workshop</field>
        <field name="stage_id" ref="event.event_stage_new"/>
        <field name="seats_max">50</field>
        <field name="user_id" ref="base.user_admin"/>
        <field name="seats_limited" eval="True"/>
        <field name="is_published" eval="True"/>
        <field name="tag_ids" eval="[(6, 0, [ref('event_tag_3'),ref('event_tag_2'),ref('event_tag_1')])]"/>
        <field name="date_begin"
            eval="datetime.today().date() + relativedelta(months=1, days=4, hours=14)"/>
        <field name="date_end"
            eval="datetime.today().date() + relativedelta(months=1, days=4, hours=18)"/>
        <!-- <field name="image_1024" type="base64" file="ngo/static/src/binary/ir_attachment/1452-unsplash_qTWZOI5bLvE_activism.jpg"/> -->
        <field name="cover_properties"><![CDATA[{"background-image":"url(/unsplash/qTWZOI5bLvE/activism.jpg)"}]]></field>
    </record>
    <record id="event_event_3" model="event.event">
        <field name="name">Refugee Welcome Pack Distribution</field>
        <field name="stage_id" ref="event.event_stage_new"/>
        <field name="seats_max">15</field>
        <field name="user_id" ref="base.user_admin"/>
        <field name="seats_limited" eval="True"/>
        <field name="date_begin" eval="datetime.today().date() + relativedelta(months=2, hours=8)"/>
        <field name="date_end" eval="datetime.today().date() + relativedelta(months=2, hours=18)"/>
    </record>
    <record id="event_event_4" model="event.event">
        <field name="name">2025 Annual General Assembly</field>
        <field name="stage_id" ref="event.event_stage_booked"/>
        <field name="user_id" ref="base.user_admin"/>
        <field name="date_begin"
            eval="datetime.today().date() + relativedelta(months=1, days=20, hours=10)"/>
        <field name="date_end"
            eval="datetime.today().date() + relativedelta(months=1, days=20, hours=16)"/>
    </record>
    <record id="event_event_5" model="event.event">
        <field name="name">Human Rights Dinner &amp; Talk</field>
        <field name="stage_id" ref="event.event_stage_booked"/>
        <field name="seats_max">500</field>
        <field name="user_id" ref="base.user_admin"/>
        <field name="seats_limited" eval="True"/>
        <field name="is_published" eval="True"/>
        <field name="tag_ids" eval="[(6, 0, [ref('event_tag_3'),ref('event_tag_2'),ref('event_tag_4')])]"/>
        <field name="date_begin"
            eval="datetime.today().date() + relativedelta(months=2, days=10, hours=17)"/>
        <field name="date_end"
            eval="datetime.today().date() + relativedelta(months=2, days=10, hours=23)"/>
        <!-- <field name="image_1024" type="base64" file="ngo/static/src/binary/ir_attachment/1450-unsplash_XzsSg0hdXpQ_humanrights.jpg"/> -->
        <field name="cover_properties"><![CDATA[{"background-image":"url(/unsplash/XzsSg0hdXpQ/humanrights.jpg)"}]]></field>
    </record>
</odoo>
