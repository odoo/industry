<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
  <record id="mailing_mailing_1" model="mailing.mailing" context="{'mail_auto_subscribe_no_notify': True}">
    <field name="subject">Email Marketing</field>
    <field name="medium_id" ref="utm.utm_medium_email"/>
    <field name="user_id" ref="base.user_admin"/>
    <field name="mailing_model_id" ref="base.model_res_partner"/>
    <field name="reply_to_mode">new</field>
    <field name="email_from" model="res.users" eval="obj().env.ref('base.user_admin').email or 'student_organization@example.com'"/>
    <field name="mailing_filter_id" ref="mailing_filter_1"/>
  </record>
</odoo>
