<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
  <function model="pos.config" name="load_onboarding_retail_scenario">
    <value name="with_demo_data" model="ir.module.module" eval="obj().search([('name', '=', 'student_organization')]).demo"/>
  </function>
  <record id="point_of_sale.pos_config_retail" model="pos.config" forcecreate="1">
    <field name="name">MyStudentOrg</field>
    <field name="available_pricelist_ids" eval="[(6, 0, [ref('product_pricelist_2'), ref('product_pricelist_3')])]"/>
    <field name="use_pricelist" eval="True"/>
  </record>
</odoo>
