<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
  <record id="point_of_sale.pos_config_retail" model="pos.config" forcecreate="1">
    <field name="name">MyStudentOrg</field>
    <field name="currency_id" ref="base.EUR"/>
    <field name="available_pricelist_ids" eval="[(6, 0, [ref('product_pricelist_1'), ref('product_pricelist_2'), ref('product_pricelist_3')])]"/>
    <field name="tip_product_id" eval="False"/>
    <field name="use_pricelist" eval="True"/>
    <field name="payment_method_ids" eval="[(6, 0, [ref('pos_payment_method_2'), ref('pos_payment_method_3'), ref('pos_payment_method_1')])]"/>
  </record>
</odoo>
