          <attribute name="widget"/>
        </xpath>
        <xpath expr="//field[@name='date_done']" position="attributes">
          <attribute name="optional">show</attribute>
        </xpath>
      </data>
    </field>
    <field name="inherit_id" ref="stock.vpicktree"/>
    <field name="mode">extension</field>
    <field name="model">stock.picking</field>
    <field name="name">Odoo Studio: stock.picking.list customization</field>
    <field name="priority">160</field>
    <field name="active" eval="True"/>
    <field name="type">list</field>
  </record>
  <record id="odoo_studio_stock_mo_5839ed5f-2c39-4ac3-b303-aff09f292a50" model="ir.ui.view">
