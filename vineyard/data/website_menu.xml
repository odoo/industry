<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <function model="website.menu" name="write">
        <value model="website.menu" eval="obj().search([
            ('website_id', '=', ref('website.default_website')),
            ('url', '=', '/appointment'),
        ]).id"/>
        <value eval="{'name': 'Winery Tour'}"/>
    </function>
</odoo>
