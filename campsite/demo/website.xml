<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
  <record id="website.default_website" model="website">
    <field name="name">campsite</field>
    <field name="configurator_done" eval="True"/>
    <field name="favicon" type="base64" file="campsite/static/description/icon.png"/>
    <field name="logo" type="base64" file="campsite/static/description/icon.png"/>
    <field name="user_id" ref="base.public_user"/>
    <field name="theme_id" search="[('id', '=', ref('base.module_theme_bistro', raise_if_not_found=False))]"/>
    <field name="salesperson_id" ref="base.user_admin"/>
    <field name="show_line_subtotals_tax_selection">tax_included</field>
    <field name="cart_recovery_mail_template_id" eval="False"/>
    <field name="shop_ppr">3</field>
    <field name="shop_gap">10px</field>
    <field name="product_page_image_width">66_pc</field>
  </record>
</odoo>
