<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="message_rating_1" model="mail.message">
        <field name="model">product.template</field>
        <field name="res_id" ref="product_template_4"/>
        <field name="body">Amazing stay at the heart of a pine tree forest. Resourcing and peaceful. Pitches are large and relatively far from each others. I can only recommend!</field>
        <field name="author_id" ref="base_industry_data.res_partner_20"/>
        <field name="message_type">comment</field>
        <field name="subtype_id" ref="mail.mt_comment"/>
    </record>   
    <record id="rating_rating_1" model="rating.rating">
        <field name="res_model_id" ref="product.model_product_template"/>
        <field name="res_id" ref="product_template_4"/>
        <field name="rating" eval="5"/>
        <field name="partner_id" ref="base_industry_data.res_partner_20"/>
        <field name="consumed" eval="True"/>
        <field name="message_id" ref="message_rating_1"/>
    </record>
</odoo>
