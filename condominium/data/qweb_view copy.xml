	<record id="web_studio_report_ed_a28f8c92-d072-488a-afda-b4730397d5ca" model="ir.ui.view">
		<field name="arch" type="xml">
			<data>
				<data>
					<xpath position="replace" expr="/t[@t-name='studio_report_document']//t[@t-call='web.external_layout']/div/div" meta-class="oe_structure" />
					<xpath position="inside" expr="/t[@t-name='studio_report_document']//t[@t-call='web.external_layout']/div" meta-class="page">
						<div class="oe_structure" style="text-align: center;">
							<h2>
								<strong>
									GENERAL MEETING AGENDA
								</strong>
							</h2>
							<h5 t-field="doc.start" />
							<h5 t-field="doc.location" />
							<br />
						</div>
						<h5 class="oe_structure">
							Expected attendees
						</h5>
						<t t-set="total_ratios" t-value="sum([x_studio_ratio.x_ratio for x_studio_ratio in doc.x_studio_voting_key.x_studio_ratio_ids]) " />
						<t t-foreach="doc.attendee_ids" t-as="attendee">
							<t t-if="attendee.x_studio_ratio &gt; 0">
								<li>
									<strong>
										<t t-out="attendee.partner_id.name" />
									</strong>
									, representing
									<span t-esc="round(attendee.x_studio_ratio / total_ratios * 100,1)" />
									% of the owner ratios;
								</li>
							</t>
						</t>
						<t t-set="motion_number" t-value="0" />
						<t t-foreach="doc.x_studio_vote_ids" t-as="motion">
							<t t-set="motion_number" t-value="motion_number + 1" />
							<br />
							<h4>
								<t t-out="motion_number" />
								.
								<t t-out="motion.x_name" />
							</h4>
						</t>
						<div class="oe_structure">
							<br />
						</div>
					</xpath>
				</data>
			</data>
		</field>
