	<record id="web_studio_report_ed_b22dc65e-fb45-4b98-91a9-e4821dc1c727" model="ir.ui.view">
		<field name="arch" type="xml">
			<data>
				<data>
					<xpath position="replace" expr="/t[@t-name='studio_report_document']//t[@t-call='web.external_layout']/div/div" meta-class="oe_structure" />
					<xpath position="inside" expr="/t[@t-name='studio_report_document']//t[@t-call='web.external_layout']/div" meta-class="page">
						<div class="oe_structure" style="text-align: center;">
							<h2>
								<strong>
									GENERAL MEETING REPORT
								</strong>
							</h2>
							<h5 t-field="doc.start" />
							<h5 t-field="doc.location" />
						</div>
						<div class="oe_structure">
							<br />
							<strong>
								<span t-field="doc.x_studio_attending_owners" />owners</strong>were present and represented<strong>
								<span t-esc="round(doc.x_studio_attending_ratio * 100,1)" />%</strong>of the owner ratios.<br /><br />
						</div>
						<h5 class="oe_structure">Attending</h5>
						<t t-set="total_ratios" t-value="sum([x_studio_ratio.x_ratio for x_studio_ratio in doc.x_studio_voting_key.x_studio_ratio_ids]) " />
						<t t-foreach="doc.attendee_ids" t-as="attendee">
							<t t-if="attendee.x_studio_attending and attendee.x_studio_ratio &gt; 0">
								<li>
									<strong><t t-out="attendee.partner_id.name" /></strong>, representing
									<span t-esc="round(attendee.x_studio_ratio / total_ratios * 100,1)" />% of the owner ratios
									<t t-if="attendee.x_studio_delegating">and delegating</t>;
								</li>
							</t>
						</t>
						<br />
						<h5 class="oe_structure">
							Excused
						</h5>
						<t t-foreach="doc.attendee_ids" t-as="attendee">
							<t t-if="not attendee.x_studio_attending and attendee.x_studio_ratio &gt; 0">
								<li>
									<strong>
										<t t-out="attendee.partner_id.name" />
									</strong>
									, representing
									<span t-esc="round(attendee.x_studio_ratio / total_ratios * 100,1)" />
									% of the owner ratios
									<t t-if="attendee.x_studio_delegating">
										<strong>
											and delegating
										</strong>
									</t>
									;
								</li>
							</t>
						</t>
						<br />
						<div class="oe_structure">
							<br />
						</div>
						<div class="oe_structure" style="text-align: center;">
							<h3><strong>Motions</strong></h3>
							<br />
						</div>
						<t t-set="motion_number" t-value="0" />
						<t t-foreach="doc.x_studio_vote_ids" t-as="motion">
							<t t-set="motion_number" t-value="motion_number + 1" />
							<h4>
								<t t-out="motion_number" />.<t t-out="motion.x_name" />
							</h4>
							<p t-field="motion.x_studio_notes" />
							<p>
								Votes (need<t t-out="motion.x_studio_vote_criteria.x_name" />):
							</p>
							<t t-if="motion.x_studio_abstention != 0">
								<li>
									<span>
										<strong style="color:grey;">
											Abstention:
											<t t-out="round(motion.x_studio_abstention * 100,1)" />
											%
										</strong>
									</span>
									<t t-if="motion.x_studio_abstention != 0 and motion.x_studio_abstention != 1">
										<span style="color:grey;">
											<t t-foreach="motion.x_studio_attendee_vote_ids" t-as="voter">
												<t t-if="voter.x_studio_vote == 'Abstention'">
													<t t-out="voter.x_studio_attendee.name" />
													,
												</t>
											</t>
										</span>
										<br />
									</t>
								</li>
							</t>
							<t t-if="motion.x_studio_against != 0">
								<li>
									<span>
										<strong style="color:red;">
											Against:
											<t t-out="round(motion.x_studio_against * 100,1)" />
											%
										</strong>
									</span>
									<t t-if="motion.x_studio_against != 0 and motion.x_studio_against != 1">
										<span style="color:grey;">
											<t t-foreach="motion.x_studio_attendee_vote_ids" t-as="voter">
												<t t-if="voter.x_studio_vote == 'Against'">
													<t t-out="voter.x_studio_attendee.name" />
													,
												</t>
											</t>
										</span>
										<br />
									</t>
								</li>
							</t>
							<t t-if="motion.x_studio_in_favor != 0">
								<li>
									<span>
										<strong style="color:green;">
											In favor:
											<t t-out="round(motion.x_studio_in_favor * 100,1)" />
											%
										</strong>
									</span>
									<t t-if="motion.x_studio_in_favor != 0 and motion.x_studio_in_favor &lt; 0.999">
										<span style="color:grey;">
											<t t-foreach="motion.x_studio_attendee_vote_ids" t-as="voter">
												<t t-if="voter.x_studio_vote == 'In Favor'">
													<t t-out="voter.x_studio_attendee.name" />
													,
												</t>
											</t>
										</span>
										<br />
									</t>
								</li>
							</t>
							<div class="oe_structure">
								<p>
									<br />
									<t t-if="motion.x_studio_outcome">
										<strong>
											The motion is
											<t t-out="motion.x_studio_outcome" />
											.
										</strong>
									</t>
									<t t-else="">
										<p>
											The motion is not submitted to vote.
										</p>
									</t>
									<br />
									<br />
								</p>
							</div>
						</t>
						<div class="oe_structure">
							<br />
						</div>
					</xpath>
				</data>
			</data>
		</field>
