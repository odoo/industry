        <field name="start" model="appointment.type"
            eval="(DateTime.today().date() + relativedelta(weekday=1, hour=9)).replace(tzinfo=pytz.timezone(obj().env.ref('bowling.appointment_type_1').appointment_tz)).astimezone(pytz.UTC).replace(tzinfo=None)" />
        <field name="stop" model="appointment.type"
            eval="(DateTime.today().date() + relativedelta(weekday=1, hour=10)).replace(tzinfo=pytz.timezone(obj().env.ref('bowling.appointment_type_1').appointment_tz)).astimezone(pytz.UTC).replace(tzinfo=None)" />
        <field name="duration">1.0</field>
        <field name="start_date" eval="DateTime.today().date()" />
        <field name="partner_ids" eval="[(6, 0, [ref('base_industry_data.res_partner_26')])]" />
        <field name="appointment_status">booked</field>
        <field name="appointment_booker_id" ref="base_industry_data.res_partner_26" />
    </record>
</odoo>
