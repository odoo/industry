<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <record id="contacts.action_contacts" model="ir.actions.act_window" forcecreate="0">
        <field name="domain">[("x_is_technical_partner", "!=", True)]</field>
    </record>
    <record id="owners_act_window" model="ir.actions.act_window">
        <field name="name">Owners</field>
        <field name="res_model">crm.lead</field>
        <field name="domain">[("team_id.x_is_sellers_team", "=", True)]</field>
        <field name="view_mode">kanban,list,form,calendar,map,pivot,graph,activity,cohort</field>
    </record>
    <record id="sale.product_template_action" model="ir.actions.act_window" forcecreate="0">
        <field name="domain">[("sale_ok", "=", True), ("x_is_properties", "=", False)]</field>
    </record>
    <record id="crm.crm_lead_action_pipeline" model="ir.actions.act_window" forcecreate="0">
        <field name="domain">[("team_id.x_is_sellers_team", "=", True)]</field>
    </record>
    <record id="buyers_act_window" model="ir.actions.act_window">
        <field name="name">Buyers</field>
        <field name="context">{'search_default_x_interested_in_id': active_id, 'default_x_interested_in_id': active_id}</field>
        <field name="domain">[('x_interested_in_id', '=', active_id)]</field>
        <field name="res_model">crm.lead</field>
        <field name="view_mode">kanban,list,form</field>
    </record>
    <record id="properties_act_window" model="ir.actions.act_window">
        <field name="name">Properties</field>
        <field name="res_model">product.template</field>
        <field name="view_mode">kanban,list,form,activity,map</field>
        <field name="domain">[("x_is_properties", "=", True)]</field>
    </record>
    <record id="map_act_window" model="ir.actions.act_window">
        <field name="name">Map</field>
        <field name="res_model">product.template</field>
        <field name="view_mode">map,kanban,list,form,activity</field>
    </record>
    <record id="properties_owner_act_window" model="ir.actions.act_window">
        <field name="name">Properties</field>
        <field name="context">{'search_default_x_owner_id': active_id, 'default_x_owner_id': active_id}</field>
        <field name="domain">[('x_owner_id', '=', active_id)]</field>
        <field name="res_model">product.template</field>
        <field name="view_mode">kanban,list,form</field>
    </record>
    <record id="opportunities_act_window" model="ir.actions.act_window">
        <field name="name">Buyers</field>
        <field name="res_model">crm.lead</field>
        <field name="view_mode">kanban,list,form,calendar,map,pivot,graph,activity,cohort</field>
        <field name="domain">[("x_interested_in_id", "!=", False)]</field>
    </record>
    <record id="matches_property_act_window" model="ir.actions.act_window">
        <field name="name">Matches</field>
        <field name="context">{'search_default_x_contact_matches_properties': active_id, 'default_x_contact_matches_properties': [active_id]}</field>
        <field name="domain">[('x_contact_matches_properties', '=', active_id)]</field>
        <field name="res_model">product.template</field>
        <field name="view_mode">kanban,list,form</field>
    </record>
    <record id="matches_contact_act_window" model="ir.actions.act_window">
        <field name="name">Matches</field>
        <field name="context">{'search_default_x_property_matches_contacts': active_id, 'default_x_property_matches_contacts': [active_id]}</field>
        <field name="domain">[('x_property_matches_contacts', '=', active_id)]</field>
        <field name="res_model">res.partner</field>
        <field name="view_mode">kanban,list,form</field>
    </record>
    <record id="visits_event_act_window" model="ir.actions.act_window">
        <field name="name">Visits</field>
        <field name="context">{'search_default_x_property_id': active_id, 'default_x_property_id': active_id}</field>
        <field name="domain">[('x_property_id', '=', active_id)]</field>
        <field name="res_model">calendar.event</field>
    </record>
    <record id="visits_appointment_act_window" model="ir.actions.act_window">
        <field name="name">Visits</field>
        <field name="res_model">appointment.type</field>
        <field name="view_mode">calendar,kanban,list,form</field>
        <field name="domain">[("x_property_id", "!=", False)]</field>
    </record>
</odoo>
