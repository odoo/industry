<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
  <record id="website.default_website" model="website" forcecreate="1">
    <field name="name">Theater</field>
    <field name="logo" type="base64" file="theater/static/description/icon.png"/>
    <field name="favicon" type="base64" file="theater/static/description/icon.png"/>
    <field name="theme_id" search="[('id', '=', ref('base.module_theme_loftspace', raise_if_not_found=False))]"/>
    <field name="cart_recovery_mail_template_id" eval="False"/>
    <field name="configurator_done" eval="True"/>
  </record>
</odoo>
