<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate='1'>
    <function name="write" model="project.task">
        <function model="project.task" name="search" eval="[[
        ('partner_id', 'in', [ref('base_industry_data.res_partner_33'), ref('base_industry_data.res_partner_32')]),
        '|',
            ('sale_line_id', 'in', [ref('sale_order_line_5'), ref('sale_order_line_11')]),
            ('date_deadline', '=', False)
        ]]"/>
        <value eval="{
            'stage_id': ref('project_task_type_3'),
            'state': '03_approved'
        }"/>
    </function> 
</odoo>
