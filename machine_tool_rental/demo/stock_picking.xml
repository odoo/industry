<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
  <function name="button_validate" model="stock.picking">
    <value model="stock.picking" eval="obj().search([
      ('location_id', '=', ref('stock.stock_location_stock')),
      ('lot_id', '!=', False),
    ]).ids"/>
  </function>
  <function name="button_validate" model="stock.picking">
    <value model="stock.picking" eval="obj().search([
      ('location_dest_id', '=', ref('stock.stock_location_company')),
      ('lot_id', '=',  ref('stock_lot_22')),
      ('state', '=', 'assigned'),
    ]).ids"/>
  </function>
</odoo>
