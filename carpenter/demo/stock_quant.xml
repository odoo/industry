<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
  <record id="stock_quant_1" model="stock.quant">
    <field name="product_id" ref="product_product_8"/>
    <field name="inventory_quantity_auto_apply">40.0</field>
  </record>
  <record id="stock_quant_2" model="stock.quant">
    <field name="product_id" ref="product_product_9"/>
    <field name="inventory_quantity_auto_apply">400.0</field>
  </record>

  <function name="action_stock_quant_relocate" model="stock.quant">
    <value eval="[ref('stock_quant_1'), ref('stock_quant_2')]"/>
  </function>
  <record id="relocate_quants_1" model="stock.quant.relocate">
      <field name="quant_ids" eval="[Command.link(ref('stock_quant_1')), Command.link(ref('stock_quant_2'))]"/>
      <field name="dest_location_id" ref="stock.stock_location_stock"/>
  </record>
  <function name="action_relocate_quants" model="stock.quant.relocate">
      <value eval="[ref('relocate_quants_1')]"/>
  </function>
</odoo>
