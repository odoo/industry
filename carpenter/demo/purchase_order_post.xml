<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <function name="button_confirm" model="purchase.order">
        <value eval="[ref('purchase_order_1'), ref('purchase_order_2')]"/>
    </function>
    <function name="write" model="stock.picking">
        <value model="stock.picking" eval="obj().search([
            ('purchase_id', '=', ref('purchase_order_1'))    
        ]).ids"/>
        <value model="project.project" eval="{'project_id': obj().search([('sale_order_id', '=', ref('sale_order_1'))])}"/>
    </function>
</odoo>
