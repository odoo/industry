<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="settings_act" model="ir.actions.act_window">
    <field name="name">Settings</field>
    <field name="res_model">stock.rule</field>
    <field name="view_mode">list</field>
  </record>
  <record id="return_act" model="ir.actions.act_window">
    <field name="name">Return</field>
    <field name="res_model">stock.picking</field>
    <field name="domain">["|", ("picking_type_id.display_name", "=", "PoS Orders"), ("state", "in", ["confirmed", "assigned", "waiting"])]</field>
    <field name="view_mode">kanban,list,form,calendar,map,activity</field>
  </record>
</odoo>
