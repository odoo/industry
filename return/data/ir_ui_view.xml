<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <record id="odoo_studio_stock_pi_d1070c6b-92a3-4b15-9b1a-2e02162d5326" model="ir.ui.view" context="{'studio': True}">
    <field name="arch" type="xml">
      <data>
        <xpath expr="//search[1]/field[@name='name']" position="before">
          <xpath expr="//field[@name='partner_id']" position="move"/>
          <xpath expr="//field[@name='product_id']" position="move"/>
        </xpath>
        <xpath expr="//search[1]/field[@name='name']" position="after">
          <field name="return_count"/>
        </xpath>
        <xpath expr="//field[@name='partner_id']" position="attributes">
          <attribute name="filter_domain"/>
        </xpath>
      </data>
    </field>
    <field name="inherit_id" ref="stock.view_picking_internal_search"/>
    <field name="mode">extension</field>
    <field name="model">stock.picking</field>
    <field name="name">Odoo Studio: stock.picking.internal.search customization</field>
    <field name="priority">99</field>
    <field name="type">search</field>
  </record>
  <record id="odoo_studio_stock_pi_97a07e8b-05d7-4c0d-8868-dcbf4b8dd2f1" model="ir.ui.view" context="{'studio': True}">
    <field name="arch" type="xml">
    <data>
      <xpath expr="//field[@name='user_id']" position="replace"/>
      <xpath expr="//field[@name='activity_ids']" position="replace"/>
      <xpath expr="//field[@name='state']" position="replace">
        <field name="move_ids" display="full" widget="statinfo" string="Products"/>
        <field name="activity_type_icon" display="full" invisible="state not in [&quot;waiting&quot;, &quot;assigned&quot;, &quot;confirmed&quot;] or ( not has_deadline_issue and scheduled_date &gt;= datetime.datetime.now().to_utc().strftime(&quot;%Y-%m-%d %H:%M:%S&quot;) )" string="Late" widget="statinfo" class="ms-auto"/>
      </xpath>
      <xpath expr="//kanban[1]/templates[1]/t[@t-name='card']/div[1]/field[@name='name']" position="replace"/>
    </data>
    </field>
    <field name="inherit_id" ref="stock.stock_picking_kanban"/>
    <field name="mode">extension</field>
    <field name="model">stock.picking</field>
    <field name="name">Odoo Studio: stock.picking.kanban customization</field>
    <field name="priority">1000</field>
    <field name="type">kanban</field>
  </record>
</odoo>
