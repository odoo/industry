<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <function name="button_choose_theme" model="ir.module.module" eval="[ref('base.module_theme_enark', raise_if_not_found=False) or ref('base.module_theme_default')]"/>

    <function model="web_editor.assets" name="make_scss_customization" context="{'website_id': ref('website.default_website')}">
        <value eval="'/website/static/src/scss/options/colors/user_color_palette.scss'"/>
        <value eval="{
                'o-color-1': '#0A58CA',
                'o-color-2': '#d2b58a',
                'o-color-3': '#f0f2f5',
                'o-color-4': '#FFFFFF',
                'o-color-5': '#141f2e',
                'menu': 1,
                'o-cc1-text': '&quot;o-color-5&quot;',
                'o-cc1-btn-primary': '&quot;o-color-1&quot;',
                'o-cc3-btn-secondary': '&quot;o-color-1&quot;',
                'o-cc1-btn-secondary': '&quot;o-color-4&quot;',
                'o-cc1-btn-secondary-border': '&quot;o-color-5&quot;'
                }"/>
    </function>

    <function model="ir.ui.view" name="write">
        <value model="ir.ui.view" eval="obj().env['website'].with_context(website_id=obj().env.ref('website.default_website').id).viewref('website.homepage').ids"/>
        <value model="ir.ui.view" eval="{'arch': obj().env.ref('corporate_gifts.homepage').arch}"/>
    </function>

    <function model="ir.ui.view" name="write">
        <value model="ir.ui.view" eval="obj().env['website'].with_context(website_id=obj().env.ref('website.default_website').id).viewref('website.contactus').id"/>
        <value model="ir.ui.view" eval="{'arch': obj().env.ref('corporate_gifts.contactus').arch.replace(
        'value=&quot;*pocket_printing_tag*&quot;', 'value=&quot;%s&quot;' % str(obj().env.ref('corporate_gifts.crm_tag_2').id)).replace(
        'value=&quot;*back_printing_tag*&quot;', 'value=&quot;%s&quot;' % str(obj().env.ref('corporate_gifts.crm_tag_3').id)).replace(
        'value=&quot;*pocket_back_printing_tag*&quot;', 'value=&quot;%s&quot;' % str(obj().env.ref('corporate_gifts.crm_tag_4').id)).replace(
        'value=&quot;*embroidery_tag*&quot;', 'value=&quot;%s&quot;' % str(obj().env.ref('corporate_gifts.crm_tag_5').id)).replace(
        'value=&quot;*other_tag*&quot;', 'value=&quot;%s&quot;' % str(obj().env.ref('sales_team.categ_oppor8').id)).replace(
        'value=&quot;*sales_team_team_sales_department*&quot;', 'value=&quot;%s&quot;' % str(obj().env.ref('sales_team.team_sales_department').id)).replace(
        'value=&quot;*base_user_admin*&quot;', 'value=&quot;%s&quot;' % str(obj().env.ref('base.user_admin').id))
        }"/>
    </function>
</odoo>
