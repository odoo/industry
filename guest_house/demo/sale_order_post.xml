<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <function name="action_confirm" model="sale.order">
        <value eval="[
            ref('sale_order_1'),
            ref('sale_order_2'),
            ref('sale_order_3'),
        ]"/>
    </function>
    <record id="pickup_wizard_1" model="rental.order.wizard">
        <field name="order_id" ref="sale_order_1"/>
        <field name="rental_wizard_line_ids" eval="[(0, 0, {'order_line_id': ref('sale_order_line_1'),'product_id': ref('product_product_17'),'qty_delivered': 1})]"/>
        <field name="status">pickup</field>
    </record>
    <record id="return_wizard_1" model="rental.order.wizard" context="{'order_line_ids': [ref('sale_order_line_1')]}">
        <field name="order_id" ref="sale_order_1"/>
        <field name="rental_wizard_line_ids" eval="[(0, 0, {'order_line_id': ref('sale_order_line_1'),'product_id': ref('product_product_17'),'qty_delivered': 1,'qty_returned': 1})]"/>
        <field name="status">return</field>
    </record>
    <record id="pickup_wizard_2" model="rental.order.wizard">
        <field name="order_id" ref="sale_order_2"/>
        <field name="rental_wizard_line_ids" eval="[(0, 0, {'order_line_id': ref('sale_order_line_2a'), 'product_id': ref('product_product_18'), 'qty_delivered': 1}), (0, 0, {'order_line_id': ref('sale_order_line_2b'), 'product_id': ref('product_product_14'), 'qty_delivered': 1})]"/>
        <field name="status">pickup</field>
    </record>
    <record id="pickup_wizard_3" model="rental.order.wizard">
        <field name="order_id" ref="sale_order_3"/>
        <field name="rental_wizard_line_ids" eval="[(0, 0, {'order_line_id': ref('sale_order_line_3'), 'product_id': ref('product_product_19'), 'qty_delivered': 1})]"/>
        <field name="status">pickup</field>
    </record>
    <function name="apply" model="rental.order.wizard">
        <value eval="[
            ref('pickup_wizard_1'),
            ref('return_wizard_1'),
            ref('pickup_wizard_2'),
            ref('pickup_wizard_3'),
        ]"/>
    </function>
    <function name="action_confirm" model="sale.order">
        <value eval="[
            ref('sale_order_4'),
            ref('sale_order_5'),
            ref('sale_order_6'),
        ]"/>
    </function>

    <record id="x_guests_line_1" model="x_guests_line">
