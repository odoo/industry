<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="portal_wizard_1" model="portal.wizard">
        <field name="partner_ids" eval="[(6, 0, [ref('base_industry_data.res_partner_28')])]" />
    </record>
    <function name="action_grant_access" model="portal.wizard.user" eval="[obj().env.ref('guest_house.portal_wizard_1').user_ids[0].id]"/>
    <record id="message_rating_1" model="mail.message">
        <field name="model">product.template</field>
        <field name="res_id" ref="product_template_17"/>
        <field name="body">Lovely hosts with a lot of care. I recommend for peaceful and comfy weekends!</field>
        <field name="author_id" ref="base_industry_data.res_partner_28"/>
        <field name="message_type">comment</field>
        <field name="subtype_id" ref="mail.mt_comment"/>
    </record>   
    <record id="rating_rating_1" model="rating.rating">
        <field name="res_model_id" ref="product.model_product_template"/>
        <field name="res_id" ref="product_template_17"/>
        <field name="rating" eval="5"/>
        <field name="partner_id" ref="base_industry_data.res_partner_28"/>
        <field name="consumed" eval="True"/>
        <field name="message_id" ref="message_rating_1"/>
    </record>
    </odoo>
