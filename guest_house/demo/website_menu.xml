<?xml version='1.0' encoding='UTF-8'?>
<odoo auto_sequence="1" noupdate="1">
  <function model="website.menu" name="unlink">
    <value model="website.menu"
      eval="obj().search([('website_id', '=', ref('website.default_website')), ('url', '=', '/appointment')]).id" />
  </function>
  <function name="write" model="website.menu">
    <value model="website.menu" 
      eval="obj().search([('website_id', '=', ref('website.default_website')), ('url', '=', '/shop')]).id" />
    <value eval="{'name': 'Rooms'}"/>
  </function>
</odoo>
