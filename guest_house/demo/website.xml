<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
  <record id="website.default_website" model="website" forcecreate="1">
    <field name="name">guest_house</field>
    <field name="configurator_done" eval="True"/>
    <field name="favicon" type="base64" file="guest_house/static/description/icon.png"/>
    <field name="logo" type="base64" file="guest_house/static/description/icon.png"/>
    <field name="user_id" ref="base.public_user"/>
    <field name="theme_id" search="[('id', '=', ref('base.module_theme_notes', raise_if_not_found=False))]"/>
    <field name="show_line_subtotals_tax_selection">tax_included</field>
    <field name="shop_ppr">3</field>
    <field name="product_page_image_layout">grid</field>
    <field name="product_page_image_width">66_pc</field>
    <field name="product_page_grid_columns">1</field>
  </record>
</odoo>
