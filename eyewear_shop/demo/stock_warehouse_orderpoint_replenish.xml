<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <function model="stock.warehouse.orderpoint" name="write">
        <value eval="[ref('eyewear_shop.orderpoint_1')]"/>
        <value eval="{'qty_to_order': 20.0}"/>
    </function>
    <function model="stock.warehouse.orderpoint" name="write">
        <value eval="[ref('eyewear_shop.orderpoint_2')]"/>
        <value eval="{'qty_to_order': 13.0}"/>
    </function>

    <function model="stock.warehouse.orderpoint" name="action_replenish">
        <value eval="[
            ref('eyewear_shop.orderpoint_1'),ref('eyewear_shop.orderpoint_2')]"
        />
    </function>
    <function name="button_mark_done" model="mrp.production">
        <value model="mrp.production" eval="
            obj().search([('orderpoint_id', '=', obj().env.ref('eyewear_shop.orderpoint_1').id)]).ids"/>
    </function>
    <function name="button_mark_done" model="mrp.production">
        <value model="mrp.production" eval="
            obj().search([('orderpoint_id', '=', obj().env.ref('eyewear_shop.orderpoint_2').id)]).ids"/>
    </function>
</odoo>
