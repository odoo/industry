<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="website.default_website" model="website" forcecreate="1">
        <field name="name">ViSiO</field>
        <field name="configurator_done" eval="True"/>
        <field name="theme_id" search="[('id', '=', ref('base.module_theme_paptic', raise_if_not_found=False))]"/>
        <field name="logo" type="base64" file="eyewear_shop/static/src/binary/website/1-logo"/>
        <field name="favicon" type="base64" file="eyewear_shop/static/src/binary/website/1-favicon"/>
    </record>
</odoo>
