<?xml version='1.0' encoding='UTF-8'?>
<odoo>
  <function name="button_choose_theme" model="ir.module.module" eval="[ref('base.module_theme_paptic', raise_if_not_found=False) or ref('base.module_theme_default')]"/>

  <!-- <function model="theme.utils" name="enable_view" eval="['website.template_header_sales_two']"/> -->
  <function model="theme.utils" name="enable_view" eval="['website.header_navbar_pills_style']"/>
  <function model="theme.utils" name="disable_view" eval="['website_blog.opt_blog_cover_post']"/>
  <function model="theme.utils" name="disable_view" eval="['website_sale.product_quantity']"/>
  <function model="theme.utils" name="disable_view" eval="['website_sale.products_attributes']"/>
  <function model="theme.utils" name="disable_view" eval="['website_sale.products_attributes_top']"/>
  <function model="theme.utils" name="disable_view" eval="['website_sale.products_categories']"/>
  <function model="theme.utils" name="disable_view" eval="['website_sale.products_categories_top']"/>
  <function model="theme.utils" name="enable_view" eval="['website_sale.header_hide_empty_cart_link']"/>
  <function model="ir.ui.view" name="write">
    <value model="ir.ui.view" eval="obj().env['website'].with_context(website_id=obj().env.ref('website.default_website').id).viewref('website.template_footer_centered').id"/>
    <value model="ir.ui.view" eval="{'arch': obj().env.ref('members_club.ir_ui_view_3646').arch}"/>
  </function>
  <function model="ir.ui.view" name="write">
    <value model="ir.ui.view" eval="obj().env['website'].with_context(website_id=obj().env.ref('website.default_website').id).viewref('website.homepage').id"/>
    <value model="ir.ui.view" eval="{'arch': obj().env.ref('members_club.homepage').arch}"/>
  </function>

  <function model="website.assets" name="make_scss_customization">
    <value eval="'/website/static/src/scss/options/user_values.scss'" />
    <value eval="{
          'menu-gradient': 'null',
          'menu-secondary-gradient': 'NULL',
          'footer-gradient': 'NULL',
          'copyright-gradient': 'null',
          'o-cc1-bg-gradient': 'null',
          'o-cc2-bg-gradient': 'null',
          'o-cc3-bg-gradient': 'null',
          'o-cc4-bg-gradient': 'null',
          'o-cc5-bg-gradient': 'null',
          'font': 'null',
          'headings-font': 'null',
          'navbar-font': 'null',
          'buttons-font': 'null',
          'color-palettes-number': 'null',
          'color-palettes-name': 'default-23',
          'btn-ripple': 'null',
          'header-template': 'sales_two',
          'footer-template': 'null',
          'footer-scrolltop': 'null',
          'header-links-style': 'pills',
          '': 'null',
          'btn-primary-outline': 'false',
          'btn-primary-flat': 'false',
          'btn-secondary-outline': 'true',
          'btn-secondary-flat': 'false',
          'btn-border-radius': '6.25rem',
          'btn-border-radius-sm': '6.25rem',
          'btn-border-radius-lg': '6.25rem',
          'btn-font-size': '0.875rem',
      }"/>
  </function>
  <function model="website.assets" name="make_scss_customization">
    <value eval="'/website/static/src/scss/options/colors/user_color_palette.scss'" />
    <value eval="{
          'footer': '2',
          'footer-custom': 'NULL',
          'o-color-1': '#985184',
          'o-color-5': '#45253C',
          'header-sales_two': '4',
          'header-sales_two-custom': 'NULL',
      }"/>
  </function>
  <function model="website.assets" name="make_scss_customization">
    <value eval="'/website/static/src/scss/options/colors/user_theme_color_palette.scss'"/>
    <value eval="{
          'success': 'null',
          'info': 'null',
          'warning': 'null',
          'danger': 'null',
      }"/>
  </function>
</odoo>
