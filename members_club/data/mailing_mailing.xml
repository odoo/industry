<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
  <record id="mailing_mailing_2" model="mailing.mailing" context="{'mail_auto_subscribe_no_notify': True}">
    <field name="subject">June 2025</field>
    <field name="user_id" ref="base.user_admin"/>
    <field name="medium_id" ref="utm.utm_medium_email"/>
    <field name="mailing_model_id" ref="base.model_res_partner"/>
    <field name="reply_to_mode">new</field>
    <field name="email_from" model="res.users" eval="obj().env.ref('base.user_admin').email or 'members_club@example.com'"/>
    <field name="mailing_domain">[("is_blacklisted", "=", False), ("grade_id", "=", False)]</field>
  </record>
</odoo>
