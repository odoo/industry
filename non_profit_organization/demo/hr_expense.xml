<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="product_product_expenses" model="product.product">
        <field name="name">Expenses</field>
        <field name="list_price">0.0</field>
        <field name="categ_id" ref="product_category_8"/>
    </record>
    <record id="hr_expense_1" model="hr.expense">
        <field name="employee_id" ref="hr_employee_9"/>
        <field name="name">Local health kit material</field>
        <field name="date" eval="DateTime.today()"/>
        <field name="product_id" ref="product_product_expenses"/>
        <field name="payment_mode">own_account</field>
        <field name="total_amount">150</field>
    </record>
    <record id="hr_expense_2" model="hr.expense">
        <field name="employee_id" ref="hr_employee_8"/>
        <field name="name">Banner printing</field>
        <field name="date" eval="DateTime.today()"/>
        <field name="product_id" ref="product_product_expenses"/>
        <field name="payment_mode">own_account</field>
        <field name="total_amount">60</field>
    </record>
    <record id="hr_expense_3" model="hr.expense">
        <field name="employee_id" ref="hr_employee_5"/>
        <field name="name">Lunch for volunteers</field>
        <field name="date" eval="DateTime.today()"/>
        <field name="product_id" ref="product_product_expenses"/>
        <field name="payment_mode">own_account</field>
        <field name="total_amount">75</field>
    </record>
    <record id="hr_expense_4" model="hr.expense">
        <field name="employee_id" ref="hr_employee_4"/>
        <field name="name">Travel to Field Site</field>
        <field name="date" eval="DateTime.today()"/>
        <field name="product_id" ref="product_product_expenses"/>
        <field name="payment_mode">own_account</field>
        <field name="total_amount">120</field>
    </record>
    <function model="hr.expense" name="action_submit" eval="[[ref('hr_expense_2'), ref('hr_expense_4')]]"/>
</odoo>
