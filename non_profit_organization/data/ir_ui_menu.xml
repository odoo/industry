<?xml version='1.0' encoding='UTF-8'?>
<odoo auto_sequence="1">
    <record id="menu_community_main_app" model="ir.ui.menu">
        <field name="name">Community</field>
        <field name="web_icon_data" type="base64" file="non_profit_organization/static/description/icon.png"/>
    </record>

    <record id="menu_members_submenu" model="ir.ui.menu">
        <field name="name">Members</field>
        <field name="parent_id" ref="menu_community_main_app"/>
        <field name="action" ref="action_window_members"/>
    </record>

    <record id="menu_donation_submenu" model="ir.ui.menu">
        <field name="name">Donation</field>
        <field name="parent_id" ref="menu_community_main_app"/>
    </record>
    <record id="menu_donation_donation" model="ir.ui.menu">
        <field name="action" ref="action_window_donations"/>
        <field name="name">Donation</field>
        <field name="parent_id" ref="menu_donation_submenu"/>
    </record>

    <record id="menu_community_configuration" model="ir.ui.menu">
        <field name="name">Configuration</field>
        <field name="parent_id" ref="menu_community_main_app"/>
    </record>
</odoo>
