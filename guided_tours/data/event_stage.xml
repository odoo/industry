<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
  <record id="event.event_stage_new" model="event.stage" forcecreate="1">
    <field name="name">Draft</field>
    <field name="description"><![CDATA[Freshly created]]></field>
  </record>
  <record id="event.event_stage_announced" model="event.stage" forcecreate="1">
    <field name="name">Published</field>
    <field name="description"><![CDATA[The event has been publicly announced]]></field>
    <field name="sequence">3</field>
  </record>
  <record id="event.event_stage_done" model="event.stage" forcecreate="1">
    <field name="name">Ended</field>
    <field name="description"><![CDATA[Finished events. Odoo will automatically move them to this stage once their end date has passed.]]></field>
    <field name="sequence">5</field>
    <field name="fold" eval="True"/>
    <field name="pipe_end" eval="True"/>
  </record>
  <record id="event.event_stage_cancelled" model="event.stage" forcecreate="1">
    <field name="name">Cancelled</field>
    <field name="description"><![CDATA[The event has been cancelled]]></field>
    <field name="sequence">6</field>
    <field name="fold" eval="True"/>
    <field name="pipe_end" eval="True"/>
  </record>
</odoo>
