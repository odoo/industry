<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="appointment_type_1" model="appointment.type">
        <field name="is_published" eval="True"/>
        <field name="location_id" ref="base.main_partner"/>
        <field name="show_avatars">False</field>
        <field name="max_schedule_days">30</field>
        <field name="slot_creation_interval">0.25</field>
        <field name="staff_user_ids" eval="[(6, 0, [])]"/>
        <field name="resource_ids" model="appointment.resource" eval="[(6, 0, obj().search([('capacity', '>', 1),('name', 'ilike', 'Table%')]).ids)]"/>
    </record>
</odoo>
