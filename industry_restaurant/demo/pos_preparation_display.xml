<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="preparation_display_main_restaurant" model="pos_preparation_display.display">
        <field name="name">Main Display</field>
        <field name="pos_config_ids" eval="[(6, 0, [ref('pos_config_main_seven_star_restaurant')])]"/>
        <field name="category_ids" eval="[
            (6, 0, [
                ref('pos_category_1'),
                ref('pos_category_2'),
                ref('pos_category_3'),
                ref('pos_category_4'),
                ref('pos_category_5'),
            ])
        ]"/>
    </record>
    <record id="preparation_display_main_restaurant_chef_special" model="pos_preparation_display.display">
        <field name="name">Chef Special</field>
        <field name="pos_config_ids" eval="[(6, 0, [ref('pos_config_main_seven_star_restaurant')])]"/>
        <field name="category_ids" eval="[(6, 0, [ref('pos_category_6')])]"/>
    </record>
</odoo>
