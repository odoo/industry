<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <function model="pos.config" name="load_onboarding_restaurant_scenario"/>
    <record id="pos_restaurant.pos_config_main_restaurant" model="pos.config" forcecreate="0">
        <field name="module_pos_hr" eval="True"/>
        <field name="use_pricelist" eval="True"/>
        <field name="available_pricelist_ids" eval="[(6, 0, [ref('product_pricelist_1'), ref('product_pricelist_2')])]"/>
        <field name="pricelist_id" ref="product_pricelist_1"/>
        <field name="module_pos_appointment" eval="True"/>
        <field name="appointment_type_id" ref="appointment_type_1"/>
        <field name="show_product_images" eval="True"/>
        <field name="minimal_employee_ids" eval="[(6, 0, [ref('hr_employee_2')])]"/>
        <field name="basic_employee_ids" eval="[(6, 0, [ref('hr_employee_5'),ref('hr_employee_6')])]"/>
        <field name="advanced_employee_ids" eval="[(6, 0, [ref('hr_employee_4'),ref('hr.employee_admin')])]"/>
    </record>
</odoo>
