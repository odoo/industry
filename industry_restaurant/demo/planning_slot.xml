<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="planning_slot_1" model="planning.slot">
        <field name="role_id" ref="planning_role_2"/>
        <field name="start_datetime" model="hr.employee" eval="
            (DateTime.today().date() + relativedelta(weekday=0, hour=11)).replace(tzinfo=pytz.timezone(obj().env.user.tz or 'UTC')).astimezone(pytz.UTC).replace(tzinfo=None)
        "/>
        <field name="end_datetime" model="hr.employee" eval="
            (DateTime.today().date() + relativedelta(weekday=0, hour=16)).replace(tzinfo=pytz.timezone(obj().env.user.tz or 'UTC')).astimezone(pytz.UTC).replace(tzinfo=None)
        "/>
        <field name="state">published</field>
        <field name="repeat" eval="True"/>
        <field name="repeat_interval">1</field>
        <field name="repeat_unit">day</field>
    </record>

