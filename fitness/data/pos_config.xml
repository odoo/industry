<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <function model="pos.config" name="load_onboarding_retail_scenario">
        <value name="with_demo_data" model="ir.module.module" eval="obj().search([('name', '=', 'fitness')]).demo"/>
    </function>
    <record id="point_of_sale.pos_config_retail" model="pos.config" forcecreate="1">
        <field name="name">Fitness Center</field>
        <field name="iface_tipproduct" eval="True"/>
        <field name="iface_print_auto" eval="True"/>
        <field name="ship_later" eval="True"/>
        <field name="down_payment_product_id" ref="pos_sale.default_downpayment_product"/>
    </record>
</odoo>
