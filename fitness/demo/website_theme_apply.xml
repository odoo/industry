<?xml version='1.0' encoding='UTF-8'?>
<odoo>
    <function name="write" model="ir.ui.view">
        <value model="ir.ui.view" eval="obj().env['website'].with_context(website_id=obj().env.ref('website.default_website').id).viewref('website.homepage').id"/>
        <value model="ir.ui.view" eval="{'arch': obj().env.ref('fitness.homepage').arch}"/>
    </function>
    <function model="ir.ui.view" name="write">
        <value model="ir.ui.view" eval="obj().env.ref('fitness.pricing').id"/>
        <value model="ir.ui.view" eval="{'arch': obj().env.ref('fitness.pricing').arch.replace(
        '*silver_membership_product*', str(obj().env.ref('fitness.product_product_7').website_url)).replace(
        '*gold_membership_product*', str(obj().env.ref('fitness.product_product_8').website_url)).replace(
        '*diamond_membership_product*', str(obj().env.ref('fitness.product_product_9').website_url))
        }"/>
    </function>
</odoo>
