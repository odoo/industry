<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
  <record id="website.default_website" model="website" forcecreate="1">
    <field name="name">My Gallery</field>
    <field name="configurator_done" eval="True"/>
    <field name="logo" type="base64" file="gallery/static/src/binary/website/1-logo"/>
    <field name="user_id" ref="base.public_user"/>
    <field name="robots_txt"><![CDATA[User-agent: *
Disallow: /]]></field>
    <field name="favicon" type="base64" file="gallery/static/src/binary/website/1-favicon"/>
    <!-- <field name="theme_id" ref="base.module_theme_nano"/> -->
    <field name="salesperson_id" ref="base.user_admin"/>
    <field name="cart_recovery_mail_template_id" eval="False"/>
    <field name="shop_ppg">18</field>
    <field name="shop_ppr">3</field>
    <field name="shop_gap">13px</field>
    <field name="prevent_zero_price_sale" eval="True"/>
    <field name="prevent_zero_price_sale_text">Contact us for Price</field>
  </record>
</odoo>
