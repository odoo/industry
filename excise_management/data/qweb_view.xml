<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="report_invoice_document_inherid_external_ids" name="External IDs" inherit_id="account.report_invoice_document">
        <xpath expr="//div[@name='reference']" position="after">
            <div class="col" t-if="o.x_external_ids" name="x_external_ids">
                <strong>External IDs</strong>
                <t t-foreach="o.x_external_ids" t-as="e_id">
                    <div t-field="e_id.x_external_id"/>
                </t>
            </div>
        </xpath>
    </template>
</odoo>
