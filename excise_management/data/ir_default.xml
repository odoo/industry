<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="active_excise_category_default" model="ir.default">
        <field name="field_id" ref="active_excise_category"/>
        <field name="json_value">true</field>
    </record>
    <record id="sequence_excise_category_default" model="ir.default">
        <field name="field_id" ref="sequence_excise_category"/>
        <field name="json_value">10</field>
    </record>
    <record id="fiscal_position_is_fiscal_deposit_default" model="ir.default">
        <field name="field_id" ref="fiscal_position_is_fiscal_deposit"/>
        <field name="json_value">true</field>
    </record>
    <record id="default_excise_report_state" model="ir.default">
        <field name="field_id" ref="state_excise_report"/>
        <field name="json_value">"progress"</field>
    </record>
    <record id="default_res_config_reporter" model="ir.default">
        <field name="field_id" ref="res_config_reporter"/>
        <field name="json_value" eval="ref('base.user_admin')"/>
    </record>
    <record id="default_excise_category_currency_id" model="ir.default">
        <field name="field_id" ref="currency_id_excise_category"/>
        <field name="json_value" model="res.company" eval="obj().env.ref('base.main_company').currency_id.id"/>
    </record>
</odoo>
