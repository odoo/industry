        <field name="model_id" ref="excise_report"/>
        <field name="name">x_from_date</field>
    </record>
    <record id="to_date_excise_report" model="ir.model.fields">
        <field name="ttype">date</field>
        <field name="required" eval="True"/>
        <field name="field_description">To Date</field>
        <field name="model_id" ref="excise_report"/>
        <field name="name">x_to_date</field>
    </record>
    <record id="name_excise_report" model="ir.model.fields">
        <field name="ttype">char</field>
        <field name="copied" eval="True"/>
        <field name="field_description">Name</field>
        <field name="model_id" ref="excise_report"/>
        <field name="name">x_name</field>
        <field name="translate">standard</field>
        <field name="depends">x_from_date</field>
        <field name="compute"><![CDATA[
for record in self:
    record['x_name'] = f"{str(record.x_from_date.isocalendar()[0])[2:]}W{record.x_from_date.isocalendar()[1]:02d}" if record.x_from_date else "Draft"
    ]]></field>
    </record>
