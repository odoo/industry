<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
  <record id="stock_warehouse_orderpoint_1" model="stock.warehouse.orderpoint">
    <field name="product_id" ref="product_product_9"/>
    <field name="product_max_qty">1000.0</field>
    <field name="product_min_qty">200.0</field>
  </record>
  <record id="stock_warehouse_orderpoint_2" model="stock.warehouse.orderpoint">
    <field name="product_id" ref="product_product_10"/>
    <field name="product_max_qty">1500.0</field>
    <field name="product_min_qty">250.0</field>
  </record>
  <record id="stock_warehouse_orderpoint_4" model="stock.warehouse.orderpoint">
    <field name="product_id" ref="product_product_4"/>
    <field name="product_max_qty">1000.0</field>
    <field name="product_min_qty">200.0</field>
    <field name="trigger">manual</field>
  </record>
  <record id="stock_warehouse_orderpoint_6" model="stock.warehouse.orderpoint">
    <field name="product_id" ref="product_product_5"/>
    <field name="product_max_qty">200.0</field>
    <field name="product_min_qty">20.0</field>
    <field name="route_id" ref="purchase_stock.route_warehouse0_buy"/>
    <field name="trigger">manual</field>
  </record>
  <!-- The next records are created with admin user to prevent the replenish rules from being unlinked in certains conditions
  (when it has been created by the superuser and qty_to_order <= 0) -->
  <function name="write" model="stock.warehouse.orderpoint">
    <value model="stock.warehouse.orderpoint" eval="[obj().with_user(ref('base.user_admin')).create({'product_id': obj().env.ref('food_distribution.product_product_2').id}).id]"/>
    <value eval="{'product_max_qty': 5000.0, 'product_min_qty': 1000.0, 'trigger': 'manual', 'product_supplier_id': ref('base_industry_data.res_partner_31')}"/>
  </function>
  <function name="write" model="stock.warehouse.orderpoint">
    <value model="stock.warehouse.orderpoint" eval="[obj().with_user(ref('base.user_admin')).create({'product_id': obj().env.ref('food_distribution.product_product_8').id}).id]"/>
    <value eval="{'product_max_qty': 500.0, 'product_min_qty': 100.0, 'trigger': 'manual', 'product_supplier_id': ref('base_industry_data.res_partner_31')}"/>
  </function>
  <function name="write" model="stock.warehouse.orderpoint">
    <value model="stock.warehouse.orderpoint" eval="[obj().with_user(ref('base.user_admin')).create({'product_id': obj().env.ref('food_distribution.product_product_12').id}).id]"/>
    <value eval="{'trigger': 'manual'}"/>
  </function>
</odoo>
