<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="production_schedule_1" model="mrp.production.schedule">
        <field name="product_id" ref="product_product_3"/>
        <field name="replenish_trigger">manual</field>
        <field name="enable_batch_size" eval="True"/>
        <field name="batch_size">200.0</field>
        <field name="is_indirect" eval="True"/>
    </record>
    <record id="production_schedule_2" model="mrp.production.schedule">
        <field name="product_id" ref="product_product_12"/>
        <field name="replenish_trigger">manual</field>
        <field name="bom_id" ref="mrp_bom_2"/>
    </record>
    <record id="production_schedule_3" model="mrp.production.schedule">
        <field name="product_id" ref="product_product_13"/>
        <field name="replenish_trigger">manual</field>
        <field name="bom_id" ref="mrp_bom_3"/>
    </record>
    <function name="write" model="mrp.production.schedule">
        <value model="mrp.production.schedule" eval="obj().search([('product_id', '=', ref('product_product_9'))]).id"/>
        <value eval="{'replenish_trigger': 'manual'}"/>
    </function>
    <function name="write" model="mrp.production.schedule">
        <value model="mrp.production.schedule" eval="obj().search([('product_id', '=', ref('product_product_10'))]).id"/>
        <value eval="{'replenish_trigger': 'manual'}"/>
    </function>

    <function name="set_forecast_qty" model="mrp.production.schedule"
        eval="(ref('production_schedule_2'), 0, 220)"/>
    <function name="set_forecast_qty" model="mrp.production.schedule"
        eval="(ref('production_schedule_2'), 1, 240)"/>
    <function name="set_forecast_qty" model="mrp.production.schedule"
        eval="(ref('production_schedule_2'), 2, 250)"/>
    <function name="set_forecast_qty" model="mrp.production.schedule"
        eval="(ref('production_schedule_2'), 3, 260)"/>
    <function name="set_forecast_qty" model="mrp.production.schedule"
        eval="(ref('production_schedule_3'), 0, 330)"/>
    <function name="set_forecast_qty" model="mrp.production.schedule"
        eval="(ref('production_schedule_3'), 1, 360)"/>
    <function name="set_forecast_qty" model="mrp.production.schedule"
        eval="(ref('production_schedule_3'), 2, 380)"/>
    <function name="set_forecast_qty" model="mrp.production.schedule"
        eval="(ref('production_schedule_3'), 3, 400)"/>

    <function name="action_replenish" model="mrp.production.schedule">
        <value eval="[
            ref('production_schedule_2'),
            ref('production_schedule_3'),
            ref('production_schedule_1'),
        ]"/>
    </function>
</odoo>
