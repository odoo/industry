<?xml version='1.0' encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="res_config_bike_shop" model="res.config.settings">
        <field name="group_analytic_accounting" eval="True"/>
        <field name="group_sale_order_template" eval="True"/>
        <field name="group_project_stages" eval="False"/>
        <field name="group_product_pricelist" eval="True"/>
        <field name="group_uom" eval="True"/>
        <field name="group_product_variant" eval="1"/>
        <field name="group_stock_production_lot" eval="1"/>
        <field name="group_lot_on_delivery_slip" eval="1"/>
    </record>
    <function name="write" model="res.config.settings">
        <value eval="[ref('res_config_bike_shop')]"/>
        <value model="res.company" eval="{'account_price_include': 'tax_included'} if not obj().env.company._existing_accounting() else {}"/>
    </function>
    <function model="res.config.settings" name="execute" eval="[ref('res_config_bike_shop')]"/>
</odoo>
