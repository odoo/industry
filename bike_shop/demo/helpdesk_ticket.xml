        <field name="user_id" ref="base.user_admin" />
        <field name="priority">2</field>
        <field name="ticket_ref">01</field>
        <field name="team_id" ref="helpdesk_team_1" />
        <field name="stage_id" ref="helpdesk_stage_10" />
    </record>
</odoo>
