<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- Confirm action -->
    <function name="action_confirm" model="sale.order">
        <value eval="[ref('sale_order_9'), ref('sale_order_13'), ref('sale_order_14'), ref('sale_order_15'), ref('sale_order_16'), ref('sale_order_10')]" />
    </function>

    <function model="project.task" name="write">
        <value model="sale.order.line" eval="obj().env.ref('bike_shop.sale_order_line_33').task_id.id"/>
        <value eval="{'user_ids': [ref('base.user_admin')], 'priority': '1' ,'stage_id': ref('industry_fsm.planning_project_stage_2'), 'tag_ids': [ref('project_tags_4'), ref('project_tags_3')]}"/>
    </function>
    <function model="project.task" name="write">
        <value model="sale.order.line" eval="obj().env.ref('bike_shop.sale_order_line_23').task_id.id"/>
        <value eval="{'user_ids': [ref('base.user_admin')], 'priority': '1' ,'stage_id': ref('industry_fsm.planning_project_stage_0'), 'tag_ids': [ref('project_tags_1'), ref('project_tags_2')]}"/>
    </function>
    <function model="project.task" name="write">
        <value model="sale.order.line" eval="obj().env.ref('bike_shop.sale_order_line_34').task_id.id"/>
        <value eval="{'user_ids': [ref('base.user_admin')],'under_warranty': '1'}"/>
    </function>

    <function model="stock.picking" name="action_assign">
        <value model="stock.picking"
            eval="obj().env['stock.picking'].search([('sale_id', '=', obj().env.ref('bike_shop.sale_order_13').id)], limit=1).id"/>
    </function>

    <function model="stock.picking" name="button_validate">
        <value model="stock.picking"
            eval="obj().env['stock.picking'].search([('sale_id', '=', obj().env.ref('bike_shop.sale_order_13').id)], limit=1).id"/>
    </function>

</odoo>
